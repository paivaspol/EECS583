  %syy.0.lcssa = phi double [ %phitmp, %._crit_edge ], [ 0.000000e+00, %._crit_edge12 ], [ 0.000000e+00, %._crit_edge12.thread ]
  %sxx.0.lcssa = phi float [ %32, %._crit_edge ], [ 0.000000e+00, %._crit_edge12 ], [ 0.000000e+00, %._crit_edge12.thread ]
