define double @sre_random_positive() #0 {
entry:
  br label %do.body

do.body:                                          ; preds = %do.body, %entry
  %call = tail call double @sre_random() #2
  %cmp = fcmp oeq double %call, 0.000000e+00
  br i1 %cmp, label %do.body, label %do.end

do.end:                                           ; preds = %do.body
  ret double %call
}
