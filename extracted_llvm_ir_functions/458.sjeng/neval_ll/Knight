define i32 @Knight(i32 %sq, i32 %c) #2 {
  tail call void @llvm.dbg.value(metadata i32 %sq, i64 0, metadata !49, metadata !176), !dbg !398
  tail call void @llvm.dbg.value(metadata i32 %c, i64 0, metadata !50, metadata !176), !dbg !399
