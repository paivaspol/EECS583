  br i1 %cmp8, label %for.body, label %for.end, !dbg !1793

for.body:                                         ; preds = %entry, %for.body
  %indvars.iv = phi i64 [ %indvars.iv.next, %for.body ], [ 0, %entry ]
  %arrayidx = getelementptr inbounds i8* %data, i64 %indvars.iv, !dbg !1794
  %0 = load i8* %arrayidx, align 1, !dbg !1794, !tbaa !918
  %arrayidx2 = getelementptr inbounds i32* %order, i64 %indvars.iv, !dbg !1794
  %1 = load i32* %arrayidx2, align 4, !dbg !1794, !tbaa !937
  %idxprom3 = sext i32 %1 to i64, !dbg !1794
  %arrayidx4 = getelementptr inbounds i8* %stream, i64 %idxprom3, !dbg !1794
  store i8 %0, i8* %arrayidx4, align 1, !dbg !1794, !tbaa !918
  %indvars.iv.next = add i64 %indvars.iv, 1, !dbg !1793
  %lftr.wideiv = trunc i64 %indvars.iv.next to i32, !dbg !1793
  %exitcond = icmp eq i32 %lftr.wideiv, %datasize, !dbg !1793
  br i1 %exitcond, label %for.end, label %for.body, !dbg !1793

for.end:                                          ; preds = %for.body, %entry
  ret void, !dbg !1795
}
