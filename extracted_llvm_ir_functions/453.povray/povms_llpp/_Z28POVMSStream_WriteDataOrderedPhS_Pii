  tail call void @llvm.dbg.value(metadata !{i8* %stream}, i64 0, metadata !354), !dbg !1792
  tail call void @llvm.dbg.value(metadata !{i32* %order}, i64 0, metadata !355), !dbg !1792
  tail call void @llvm.dbg.value(metadata !{i32 %datasize}, i64 0, metadata !356), !dbg !1792
  tail call void @llvm.dbg.value(metadata !71, i64 0, metadata !357), !dbg !1793
