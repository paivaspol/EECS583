define void @_Z27POVMSStream_ReadDataOrderedPhS_Pii(i8* nocapture readonly %stream, i8* nocapture %data, i32* nocapture readonly %order, i32 %datasize) #0 {
  tail call void @llvm.dbg.value(metadata i8* %stream, i64 0, metadata !318, metadata !1201), !dbg !2845
  tail call void @llvm.dbg.value(metadata i8* %data, i64 0, metadata !319, metadata !1201), !dbg !2846
  tail call void @llvm.dbg.value(metadata i32* %order, i64 0, metadata !320, metadata !1201), !dbg !2847
  tail call void @llvm.dbg.value(metadata i32 %datasize, i64 0, metadata !321, metadata !1201), !dbg !2848
  tail call void @llvm.dbg.value(metadata i32 0, i64 0, metadata !322, metadata !1201), !dbg !2849
  %1 = icmp sgt i32 %datasize, 0, !dbg !2850
  br i1 %1, label %.lr.ph, label %._crit_edge, !dbg !2851

.lr.ph:                                           ; preds = %0
  %2 = add i32 %datasize, -1, !dbg !2851
  br label %3, !dbg !2851

; <label>:3                                       ; preds = %3, %.lr.ph
  %indvars.iv = phi i64 [ 0, %.lr.ph ], [ %indvars.iv.next, %3 ]
  %4 = getelementptr inbounds i8* %stream, i64 %indvars.iv, !dbg !2852
  %5 = load i8* %4, align 1, !dbg !2852, !tbaa !1562
  %6 = getelementptr inbounds i32* %order, i64 %indvars.iv, !dbg !2853
  %7 = load i32* %6, align 4, !dbg !2853, !tbaa !1260
  %8 = sext i32 %7 to i64, !dbg !2854
  %9 = getelementptr inbounds i8* %data, i64 %8, !dbg !2854
  store i8 %5, i8* %9, align 1, !dbg !2855, !tbaa !1562
  %indvars.iv.next = add nuw nsw i64 %indvars.iv, 1, !dbg !2851
  %lftr.wideiv = trunc i64 %indvars.iv to i32, !dbg !2851
  %exitcond = icmp eq i32 %lftr.wideiv, %2, !dbg !2851
  br i1 %exitcond, label %._crit_edge, label %3, !dbg !2851

._crit_edge:                                      ; preds = %3, %0
  ret void, !dbg !2856
}
