define void @_Z30POVMSStream_WriteDataUnorderedPhS_i(i8* nocapture %data, i8* nocapture %stream, i32 %datasize) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{i8* %data}, i64 0, metadata !360), !dbg !1796
  tail call void @llvm.dbg.value(metadata !{i8* %stream}, i64 0, metadata !361), !dbg !1796
  tail call void @llvm.dbg.value(metadata !{i32 %datasize}, i64 0, metadata !362), !dbg !1796
  tail call void @llvm.dbg.value(metadata !71, i64 0, metadata !363), !dbg !1797
  %cmp6 = icmp sgt i32 %datasize, 0, !dbg !1797
  br i1 %cmp6, label %for.body, label %for.end, !dbg !1797

for.body:                                         ; preds = %entry, %for.body
  %indvars.iv = phi i64 [ %indvars.iv.next, %for.body ], [ 0, %entry ]
  %arrayidx = getelementptr inbounds i8* %data, i64 %indvars.iv, !dbg !1798
  %0 = load i8* %arrayidx, align 1, !dbg !1798, !tbaa !918
  %arrayidx2 = getelementptr inbounds i8* %stream, i64 %indvars.iv, !dbg !1798
  store i8 %0, i8* %arrayidx2, align 1, !dbg !1798, !tbaa !918
  %indvars.iv.next = add i64 %indvars.iv, 1, !dbg !1797
  %lftr.wideiv = trunc i64 %indvars.iv.next to i32, !dbg !1797
  %exitcond = icmp eq i32 %lftr.wideiv, %datasize, !dbg !1797
  br i1 %exitcond, label %for.end, label %for.body, !dbg !1797

for.end:                                          ; preds = %for.body, %entry
  ret void, !dbg !1799
}
