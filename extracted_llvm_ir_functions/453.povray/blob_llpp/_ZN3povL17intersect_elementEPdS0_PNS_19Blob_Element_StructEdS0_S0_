  %retval.0 = phi i32 [ 1, %sw.epilog ], [ 0, %_ZN3povL18intersect_cylinderEPNS_19Blob_Element_StructEPdS2_dS2_S2_.exit ], [ 0, %sw.bb ], [ 0, %if.end.i14 ], [ 0, %sw.bb1 ], [ 0, %if.end.i48 ], [ 0, %if.then.i72 ], [ 0, %if.end17.i ], [ 0, %if.else50.i ], [ 0, %if.end72.i ]
