  %antialias_line.2 = phi i32 [ %antialias_line.066, %if.then18 ], [ %antialias_line.066, %if.then ], [ %antialias_line.1, %for.end ]
  %63 = load i32* @_ZN3pov19Current_Line_NumberE, align 4, !dbg !2304, !tbaa !1664
  %inc47 = add nsw i32 %63, 1, !dbg !2304
  store i32 %inc47, i32* @_ZN3pov19Current_Line_NumberE, align 4, !dbg !2304, !tbaa !1664
  %64 = load i32* getelementptr inbounds (%"struct.pov::OPTIONS_STRUCT"* @_ZN3pov4optsE, i64 0, i32 25), align 4, !dbg !2304, !tbaa !1664
  %cmp13 = icmp slt i32 %inc47, %64, !dbg !2304
  br i1 %cmp13, label %for.body, label %for.cond.for.end48_crit_edge, !dbg !2304

for.cond.for.end48_crit_edge:                     ; preds = %for.inc46
  %.pre = load i32* getelementptr inbounds (%"struct.pov::OPTIONS_STRUCT"* @_ZN3pov4optsE, i64 0, i32 24), align 8, !dbg !2383, !tbaa !1664
  br label %for.end48, !dbg !2304

for.end48:                                        ; preds = %for.cond.for.end48_crit_edge, %land.end
  %65 = phi i32 [ %.pre, %for.cond.for.end48_crit_edge ], [ %8, %land.end ]
  %66 = phi i32 [ %64, %for.cond.for.end48_crit_edge ], [ %11, %land.end ]
  store i32 0, i32* @_ZN3pov19Current_Line_NumberE, align 4, !dbg !2384, !tbaa !1664
  %cmp49 = icmp eq i32 %66, %65, !dbg !2383
  br i1 %cmp49, label %if.end52, label %if.then50, !dbg !2383

if.then50:                                        ; preds = %for.end48
  %67 = load [5 x float]** @_ZN3pov13Previous_LineE, align 8, !dbg !2385, !tbaa !1667
  %sub51 = add nsw i32 %66, -1, !dbg !2385
  call void @_ZN3pov46output_single_image_line_with_alpha_correctionEPA5_fi([5 x float]* %67, i32 %sub51) #8, !dbg !2385
  br label %if.end52, !dbg !2387

if.end52:                                         ; preds = %for.end48, %if.then50
  ret void, !dbg !2388
}
