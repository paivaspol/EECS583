define i32 @bitmap_bit_p(%struct.bitmap_head_def* nocapture %head, i32 %bit) #0 {
entry:
  %call = tail call fastcc %struct.bitmap_element_def* @bitmap_find_bit(%struct.bitmap_head_def* %head, i32 %bit) #7
  %cmp = icmp eq %struct.bitmap_element_def* %call, null
  br i1 %cmp, label %return, label %if.end

if.end:                                           ; preds = %entry
  %rem = and i32 %bit, 63
  %div = lshr i32 %bit, 6
  %rem1 = and i32 %div, 1
  %idxprom = zext i32 %rem1 to i64
  %arrayidx = getelementptr inbounds %struct.bitmap_element_def* %call, i64 0, i32 3, i64 %idxprom
  %0 = load i64* %arrayidx, align 8, !tbaa !4
  %sh_prom = zext i32 %rem to i64
  %shr = lshr i64 %0, %sh_prom
  %shr.tr = trunc i64 %shr to i32
  %conv = and i32 %shr.tr, 1
  br label %return

return:                                           ; preds = %entry, %if.end
  %retval.0 = phi i32 [ %conv, %if.end ], [ 0, %entry ]
  ret i32 %retval.0
}
