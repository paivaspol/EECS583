define i32 @get_pool_mode(%struct.rtx_def* nocapture %addr) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{%struct.rtx_def* %addr}, i64 0, metadata !1695), !dbg !3722
  %0 = load %struct.function** @cfun, align 8, !dbg !3723, !tbaa !2357
  %call = tail call fastcc %struct.pool_constant* @find_pool_constant(%struct.function* %0, %struct.rtx_def* %addr) #11, !dbg !3723
  %mode = getelementptr inbounds %struct.pool_constant* %call, i64 0, i32 4, !dbg !3723
  %1 = load i32* %mode, align 4, !dbg !3723, !tbaa !2809
  ret i32 %1, !dbg !3723
}
