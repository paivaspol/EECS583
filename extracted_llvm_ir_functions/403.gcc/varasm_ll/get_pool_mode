define i32 @get_pool_mode(%struct.rtx_def* nocapture %addr) #0 {
entry:
  %0 = load %struct.function** @cfun, align 8, !tbaa !3
  %call = tail call fastcc %struct.pool_constant* @find_pool_constant(%struct.function* %0, %struct.rtx_def* %addr) #10
  %mode = getelementptr inbounds %struct.pool_constant* %call, i64 0, i32 4
  %1 = load i32* %mode, align 4, !tbaa !9
  ret i32 %1
}
