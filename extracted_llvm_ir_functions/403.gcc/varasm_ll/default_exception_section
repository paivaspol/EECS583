define void @default_exception_section() #0 {
entry:
  %0 = load i8* getelementptr inbounds (%struct.gcc_target* @targetm, i64 0, i32 13), align 8, !tbaa !5, !range !6
  %tobool = icmp eq i8 %0, 0
  br i1 %tobool, label %if.else, label %if.then

if.then:                                          ; preds = %entry
  %1 = load i32 (%union.tree_node*, i8*, i32)** getelementptr inbounds (%struct.gcc_target* @targetm, i64 0, i32 12), align 8, !tbaa !3
  %call.i = tail call i32 %1(%union.tree_node* null, i8* getelementptr inbounds ([18 x i8]* @.str11, i64 0, i64 0), i32 0) #9
  tail call void @named_section_flags(i8* getelementptr inbounds ([18 x i8]* @.str11, i64 0, i64 0), i32 %call.i) #9
  br label %if.end4

if.else:                                          ; preds = %entry
  %2 = load i32* @flag_pic, align 4, !tbaa !4
  %tobool1 = icmp eq i32 %2, 0
  br i1 %tobool1, label %if.else3, label %if.then2

if.then2:                                         ; preds = %if.else
  tail call void @data_section() #10
  br label %if.end4

if.else3:                                         ; preds = %if.else
  tail call void @const_section() #9
  br label %if.end4

if.end4:                                          ; preds = %if.then2, %if.else3, %if.then
  ret void
}
