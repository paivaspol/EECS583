define void @named_section(%union.tree_node* %decl, i8* %name, i32 %reloc) #0 {
entry:
  %cmp = icmp ne %union.tree_node* %decl, null
  br i1 %cmp, label %land.lhs.true, label %if.end

land.lhs.true:                                    ; preds = %entry
  %code = getelementptr inbounds %union.tree_node* %decl, i64 0, i32 0, i32 0, i32 2
  %0 = bitcast [4 x i8]* %code to i32*
  %bf.load = load i32* %0, align 8
  %bf.clear = and i32 %bf.load, 255
  %idxprom28 = zext i32 %bf.clear to i64
  %arrayidx = getelementptr inbounds [256 x i8]* @tree_code_type, i64 0, i64 %idxprom28
  %1 = load i8* %arrayidx, align 1, !tbaa !1
  %cmp1 = icmp eq i8 %1, 100
  br i1 %cmp1, label %if.end, label %if.then

if.then:                                          ; preds = %land.lhs.true
  tail call void @fancy_abort(i8* getelementptr inbounds ([50 x i8]* @.str4, i64 0, i64 0), i32 440, i8* getelementptr inbounds ([14 x i8]* @__FUNCTION__.named_section, i64 0, i64 0)) #12
  unreachable

if.end:                                           ; preds = %land.lhs.true, %entry
  %cmp3 = icmp eq i8* %name, null
  br i1 %cmp3, label %if.then5, label %if.end7

if.then5:                                         ; preds = %if.end
  %section_name = getelementptr inbounds %union.tree_node* %decl, i64 0, i32 0, i32 15
  %2 = load %union.tree_node** %section_name, align 8, !tbaa !3
  %pointer = getelementptr inbounds %union.tree_node* %2, i64 0, i32 0, i32 4
  %3 = load %union.tree_node** %pointer, align 8
  %4 = bitcast %union.tree_node* %3 to i8*
  br label %if.end7

if.end7:                                          ; preds = %if.then5, %if.end
  %name.addr.0 = phi i8* [ %4, %if.then5 ], [ %name, %if.end ]
  %5 = load i32 (%union.tree_node*, i8*, i32)** getelementptr inbounds (%struct.gcc_target* @targetm, i64 0, i32 12), align 8, !tbaa !3
  %call = tail call i32 %5(%union.tree_node* %decl, i8* %name.addr.0, i32 %reloc) #9
  br i1 %cmp, label %land.lhs.true8, label %if.end16

land.lhs.true8:                                   ; preds = %if.end7
  %call9 = tail call zeroext i1 @set_named_section_flags(i8* %name.addr.0, i32 %call) #10
  br i1 %call9, label %if.end16, label %if.then10

if.then10:                                        ; preds = %land.lhs.true8
  %call11 = tail call i32 @get_named_section_flags(i8* %name.addr.0) #10
  %and = and i32 %call11, 131072
  %cmp12 = icmp eq i32 %and, 0
  br i1 %cmp12, label %if.then14, label %if.end16

if.then14:                                        ; preds = %if.then10
  tail call void (%union.tree_node*, i8*, ...)* @error_with_decl(%union.tree_node* %decl, i8* getelementptr inbounds ([34 x i8]* @.str5, i64 0, i64 0)) #9
  br label %if.end16

if.end16:                                         ; preds = %if.then10, %if.then14, %land.lhs.true8, %if.end7
  %flags.0 = phi i32 [ %call, %land.lhs.true8 ], [ %call11, %if.then14 ], [ %call11, %if.then10 ], [ %call, %if.end7 ]
  tail call void @named_section_flags(i8* %name.addr.0, i32 %flags.0) #10
  ret void
}
