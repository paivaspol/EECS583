define void @bss_section() #0 {
entry:
  %0 = load i32* @in_section, align 4, !dbg !2436, !tbaa !2352
  %cmp = icmp eq i32 %0, 4, !dbg !2436
  br i1 %cmp, label %if.end, label %if.then, !dbg !2436

if.then:                                          ; preds = %entry
  %1 = load %struct._IO_FILE** @asm_out_file, align 8, !dbg !2437, !tbaa !2357
  %call = tail call i32 (%struct._IO_FILE*, i8*, ...)* @fprintf(%struct._IO_FILE* %1, i8* getelementptr inbounds ([4 x i8]* @.str, i64 0, i64 0), i8* getelementptr inbounds ([6 x i8]* @.str6, i64 0, i64 0)) #10, !dbg !2437
  store i32 4, i32* @in_section, align 4, !dbg !2439, !tbaa !2352
  br label %if.end, !dbg !2440

if.end:                                           ; preds = %entry, %if.then
  ret void, !dbg !2441
}
