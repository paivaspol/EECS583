define void @set_std_prefix(i8* nocapture %prefix, i32 %len) #0 {
entry:
  %add.i = add nsw i32 %len, 1
  %conv.i = sext i32 %add.i to i64
  %call.i = tail call noalias i8* @xmalloc(i64 %conv.i) #6
  %conv1.i = sext i32 %len to i64
  tail call void @llvm.memcpy.p0i8.p0i8.i64(i8* %call.i, i8* %prefix, i64 %conv1.i, i32 1, i1 false) #4
  %arrayidx.i = getelementptr inbounds i8* %call.i, i64 %conv1.i
  store i8 0, i8* %arrayidx.i, align 1, !tbaa !1
  store i8* %call.i, i8** @std_prefix, align 8, !tbaa !0
  ret void
}
