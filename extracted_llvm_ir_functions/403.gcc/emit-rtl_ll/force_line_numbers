define i32 @force_line_numbers() #0 {
entry:
  %0 = load i32* @no_line_numbers, align 4, !tbaa !0
  store i32 0, i32* @no_line_numbers, align 4, !tbaa !0
  %tobool = icmp eq i32 %0, 0
  br i1 %tobool, label %if.end, label %if.then

if.then:                                          ; preds = %entry
  %1 = load %struct.function** @cfun, align 8, !tbaa !4
  %emit.i = getelementptr inbounds %struct.function* %1, i64 0, i32 3
  %2 = load %struct.emit_status** %emit.i, align 8, !tbaa !4
  %x_last_linenum.i = getelementptr inbounds %struct.emit_status* %2, i64 0, i32 7
  store i32 -1, i32* %x_last_linenum.i, align 4, !tbaa !0
  br label %if.end

if.end:                                           ; preds = %entry, %if.then
  ret i32 %0
}
