define i32 @force_line_numbers() #0 {
entry:
  %0 = load i32* @no_line_numbers, align 4, !dbg !4340, !tbaa !2263
  tail call void @llvm.dbg.value(metadata !{i32 %0}, i64 0, metadata !2071), !dbg !4340
  store i32 0, i32* @no_line_numbers, align 4, !dbg !4341, !tbaa !2263
  %tobool = icmp eq i32 %0, 0, !dbg !4342
  br i1 %tobool, label %if.end, label %if.then, !dbg !4342

if.then:                                          ; preds = %entry
  %1 = load %struct.function** @cfun, align 8, !dbg !4343, !tbaa !2272
  %emit.i = getelementptr inbounds %struct.function* %1, i64 0, i32 3, !dbg !4343
  %2 = load %struct.emit_status** %emit.i, align 8, !dbg !4343, !tbaa !2272
  %x_last_linenum.i = getelementptr inbounds %struct.emit_status* %2, i64 0, i32 7, !dbg !4343
  store i32 -1, i32* %x_last_linenum.i, align 4, !dbg !4343, !tbaa !2263
  br label %if.end, !dbg !4344

if.end:                                           ; preds = %entry, %if.then
  ret i32 %0, !dbg !4345
}
