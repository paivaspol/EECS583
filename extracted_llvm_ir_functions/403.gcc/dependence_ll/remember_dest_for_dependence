define void @remember_dest_for_dependence(%union.tree_node* %node) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %node}, i64 0, metadata !911), !dbg !1759
  %tobool = icmp eq %union.tree_node* %node, null, !dbg !1760
  br i1 %tobool, label %if.end15, label %if.then, !dbg !1760

if.then:                                          ; preds = %entry
  %code = getelementptr inbounds %union.tree_node* %node, i64 0, i32 0, i32 0, i32 2, !dbg !1761
  %0 = bitcast [4 x i8]* %code to i32*, !dbg !1761
  %bf.load = load i32* %0, align 8, !dbg !1761
  %bf.clear = and i32 %bf.load, 255, !dbg !1761
  %cmp = icmp eq i32 %bf.clear, 41, !dbg !1761
  br i1 %cmp, label %land.lhs.true, label %if.end, !dbg !1761

land.lhs.true:                                    ; preds = %if.then
  %operands = getelementptr inbounds %union.tree_node* %node, i64 0, i32 0, i32 2, !dbg !1761
  %arrayidx = getelementptr inbounds i32* %operands, i64 2, !dbg !1761
  %1 = bitcast i32* %arrayidx to %union.tree_node**, !dbg !1761
  %2 = load %union.tree_node** %1, align 8, !dbg !1761, !tbaa !1257
  %tobool1 = icmp eq %union.tree_node* %2, null, !dbg !1761
  br i1 %tobool1, label %if.end, label %land.lhs.true2, !dbg !1761

land.lhs.true2:                                   ; preds = %land.lhs.true
  %code7 = getelementptr inbounds %union.tree_node* %2, i64 0, i32 0, i32 0, i32 2, !dbg !1761
  %3 = bitcast [4 x i8]* %code7 to i32*, !dbg !1761
  %bf.load8 = load i32* %3, align 8, !dbg !1761
  %bf.clear9 = and i32 %bf.load8, 255, !dbg !1761
  %cmp10 = icmp eq i32 %bf.clear9, 43, !dbg !1761
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %2}, i64 0, metadata !911), !dbg !1763
  %.node = select i1 %cmp10, %union.tree_node* %2, %union.tree_node* %node, !dbg !1761
  br label %if.end, !dbg !1761

if.end:                                           ; preds = %land.lhs.true2, %land.lhs.true, %if.then
  %node.addr.0 = phi %union.tree_node* [ %node, %land.lhs.true ], [ %node, %if.then ], [ %.node, %land.lhs.true2 ]
  store %union.tree_node* %node.addr.0, %union.tree_node** @dest_to_remember, align 8, !dbg !1764, !tbaa !1257
  br label %if.end15, !dbg !1765

if.end15:                                         ; preds = %entry, %if.end
  ret void, !dbg !1766
}
