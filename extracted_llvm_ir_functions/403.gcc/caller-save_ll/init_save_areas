define void @init_save_areas() #0 {
entry:
  %0 = load i32* @target_flags, align 4, !tbaa !3
  %and13 = lshr i32 %0, 23
  %1 = and i32 %and13, 4
  %2 = add i32 %1, 4
  %div14 = sdiv i32 16, %2
  %cmp215 = icmp slt i32 %div14, 1
  %3 = icmp sgt i32 %div14, 1
  %div14.op = add i32 %div14, -1
  %4 = zext i32 %div14.op to i64
  %.op = shl nuw nsw i64 %4, 3
  %.op.op = add i64 %.op, 8
  %5 = select i1 %3, i64 %.op.op, i64 8
  br label %for.cond1.preheader

for.cond1.preheader:                              ; preds = %for.inc6, %entry
  %indvar = phi i64 [ 0, %entry ], [ %indvar.next, %for.inc6 ]
  br i1 %cmp215, label %for.inc6, label %for.body3.lr.ph

for.body3.lr.ph:                                  ; preds = %for.cond1.preheader
  %scevgep = getelementptr [53 x [5 x %struct.rtx_def*]]* @regno_save_mem, i64 0, i64 %indvar, i64 1
  %scevgep18 = bitcast %struct.rtx_def** %scevgep to i8*
  call void @llvm.memset.p0i8.i64(i8* %scevgep18, i8 0, i64 %5, i32 8, i1 false)
  br label %for.inc6

for.inc6:                                         ; preds = %for.body3.lr.ph, %for.cond1.preheader
  %indvar.next = add i64 %indvar, 1
  %lftr.wideiv = trunc i64 %indvar.next to i32
  %exitcond = icmp eq i32 %lftr.wideiv, 53
  br i1 %exitcond, label %for.end8, label %for.cond1.preheader

for.end8:                                         ; preds = %for.inc6
  ret void
}
