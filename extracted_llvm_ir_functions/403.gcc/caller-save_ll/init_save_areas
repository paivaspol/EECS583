define void @init_save_areas() #0 {
entry:
  tail call void @llvm.dbg.value(metadata !296, i64 0, metadata !452), !dbg !736
  %0 = load i32* @target_flags, align 4, !dbg !738, !tbaa !669
  %and13 = lshr i32 %0, 23, !dbg !738
  %1 = and i32 %and13, 4, !dbg !738
  %2 = add i32 %1, 4, !dbg !738
  %div14 = sdiv i32 16, %2, !dbg !738
  %cmp215 = icmp slt i32 %div14, 1, !dbg !738
  %3 = icmp sgt i32 %div14, 1
  %div14.op = add i32 %div14, -1, !dbg !736
  %4 = zext i32 %div14.op to i64
  %.op = shl nuw nsw i64 %4, 3, !dbg !736
  %.op.op = add i64 %.op, 8, !dbg !736
  %5 = select i1 %3, i64 %.op.op, i64 8, !dbg !736
  br label %for.cond1.preheader, !dbg !736

for.cond1.preheader:                              ; preds = %for.inc6, %entry
  %indvar = phi i64 [ 0, %entry ], [ %indvar.next, %for.inc6 ]
  br i1 %cmp215, label %for.inc6, label %for.body3.lr.ph, !dbg !738

for.body3.lr.ph:                                  ; preds = %for.cond1.preheader
