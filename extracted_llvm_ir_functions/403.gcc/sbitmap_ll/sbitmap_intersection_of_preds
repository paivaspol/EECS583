define void @sbitmap_intersection_of_preds(%struct.simple_bitmap_def* nocapture %dst, %struct.simple_bitmap_def** nocapture %src, i32 %bb) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{%struct.simple_bitmap_def* %dst}, i64 0, metadata !411), !dbg !718
  tail call void @llvm.dbg.value(metadata !{%struct.simple_bitmap_def** %src}, i64 0, metadata !412), !dbg !719
  tail call void @llvm.dbg.value(metadata !{i32 %bb}, i64 0, metadata !413), !dbg !720
  %idxprom = sext i32 %bb to i64, !dbg !721
  %0 = load %struct.varray_head_tag** @basic_block_info, align 8, !dbg !721, !tbaa !578
  %data = getelementptr inbounds %struct.varray_head_tag* %0, i64 0, i32 4, !dbg !721
  %bb1 = bitcast %union.varray_data_tag* %data to [1 x %struct.basic_block_def*]*, !dbg !721
  %arrayidx = getelementptr inbounds [1 x %struct.basic_block_def*]* %bb1, i64 0, i64 %idxprom, !dbg !721
  %1 = load %struct.basic_block_def** %arrayidx, align 8, !dbg !721, !tbaa !578
  tail call void @llvm.dbg.value(metadata !{%struct.basic_block_def* %1}, i64 0, metadata !414), !dbg !721
  %size = getelementptr inbounds %struct.simple_bitmap_def* %dst, i64 0, i32 1, !dbg !722
  %2 = load i32* %size, align 4, !dbg !722, !tbaa !559
  tail call void @llvm.dbg.value(metadata !{i32 %2}, i64 0, metadata !415), !dbg !722
  %pred = getelementptr inbounds %struct.basic_block_def* %1, i64 0, i32 4, !dbg !723
  %e.056 = load %struct.edge_def** %pred, align 8, !dbg !723
  %cond57 = icmp eq %struct.edge_def* %e.056, null, !dbg !723
  br i1 %cond57, label %if.then8, label %for.body, !dbg !723

for.cond:                                         ; preds = %for.body
  %pred_next = getelementptr inbounds %struct.edge_def* %e.058, i64 0, i32 0, !dbg !723
  %e.0 = load %struct.edge_def** %pred_next, align 8, !dbg !723
  %cond = icmp eq %struct.edge_def* %e.0, null, !dbg !723
  br i1 %cond, label %if.then8, label %for.body, !dbg !723

for.body:                                         ; preds = %entry, %for.cond
  %e.058 = phi %struct.edge_def* [ %e.0, %for.cond ], [ %e.056, %entry ]
  %src2 = getelementptr inbounds %struct.edge_def* %e.058, i64 0, i32 2, !dbg !725
  %3 = load %struct.basic_block_def** %src2, align 8, !dbg !725, !tbaa !578
  %cmp3 = icmp eq %struct.basic_block_def* %3, getelementptr inbounds ([2 x %struct.basic_block_def]* @entry_exit_blocks, i64 0, i64 0), !dbg !725
  br i1 %cmp3, label %for.cond, label %for.cond10.preheader, !dbg !725

for.cond10.preheader:                             ; preds = %for.body
  %index = getelementptr inbounds %struct.basic_block_def* %3, i64 0, i32 11, !dbg !727
  %4 = load i32* %index, align 4, !dbg !727, !tbaa !559
  %idxprom5 = sext i32 %4 to i64, !dbg !727
  %arrayidx6 = getelementptr inbounds %struct.simple_bitmap_def** %src, i64 %idxprom5, !dbg !727
  %5 = load %struct.simple_bitmap_def** %arrayidx6, align 8, !dbg !727, !tbaa !578
  tail call void @llvm.dbg.value(metadata !{%struct.simple_bitmap_def* %dst}, i64 0, metadata !728) #2, !dbg !729
  tail call void @llvm.dbg.value(metadata !{%struct.simple_bitmap_def* %5}, i64 0, metadata !730) #2, !dbg !729
  %elms.i = getelementptr inbounds %struct.simple_bitmap_def* %dst, i64 0, i32 3, !dbg !731
  %6 = bitcast [1 x i64]* %elms.i to i8*, !dbg !731
  %elms1.i = getelementptr inbounds %struct.simple_bitmap_def* %5, i64 0, i32 3, !dbg !731
  %7 = bitcast [1 x i64]* %elms1.i to i8*, !dbg !731
  %conv.i = zext i32 %2 to i64, !dbg !731
  %mul.i = shl nuw nsw i64 %conv.i, 3, !dbg !731
  tail call void @llvm.memcpy.p0i8.p0i8.i64(i8* %6, i8* %7, i64 %mul.i, i32 8, i1 false) #2, !dbg !731
  %e.1.in51 = getelementptr inbounds %struct.edge_def* %e.058, i64 0, i32 0, !dbg !732
  %e.152 = load %struct.edge_def** %e.1.in51, align 8, !dbg !732
  %cmp1153 = icmp eq %struct.edge_def* %e.152, null, !dbg !732
  br i1 %cmp1153, label %if.end32, label %for.body12.lr.ph, !dbg !732

for.body12.lr.ph:                                 ; preds = %for.cond10.preheader
  %arraydecay22 = getelementptr inbounds %struct.simple_bitmap_def* %dst, i64 0, i32 3, i64 0, !dbg !733
  %cmp2447 = icmp eq i32 %2, 0, !dbg !734
  br label %for.body12, !dbg !732

if.then8:                                         ; preds = %entry, %for.cond
  tail call void @sbitmap_ones(%struct.simple_bitmap_def* %dst) #9, !dbg !736
  br label %if.end32, !dbg !736

for.body12:                                       ; preds = %for.body12.lr.ph, %for.cond10.backedge
  %e.154 = phi %struct.edge_def* [ %e.152, %for.body12.lr.ph ], [ %e.1, %for.cond10.backedge ]
  %src13 = getelementptr inbounds %struct.edge_def* %e.154, i64 0, i32 2, !dbg !737
  %8 = load %struct.basic_block_def** %src13, align 8, !dbg !737, !tbaa !578
  %cmp14 = icmp eq %struct.basic_block_def* %8, getelementptr inbounds ([2 x %struct.basic_block_def]* @entry_exit_blocks, i64 0, i64 0), !dbg !737
  %brmerge = or i1 %cmp14, %cmp2447, !dbg !737
  br i1 %brmerge, label %for.cond10.backedge, label %for.body25.lr.ph, !dbg !737

for.cond10.backedge:                              ; preds = %for.body12, %for.body25
  %e.1.in = getelementptr inbounds %struct.edge_def* %e.154, i64 0, i32 0, !dbg !732
  %e.1 = load %struct.edge_def** %e.1.in, align 8, !dbg !732
  %cmp11 = icmp eq %struct.edge_def* %e.1, null, !dbg !732
  br i1 %cmp11, label %if.end32, label %for.body12, !dbg !732

for.body25.lr.ph:                                 ; preds = %for.body12
  %index18 = getelementptr inbounds %struct.basic_block_def* %8, i64 0, i32 11, !dbg !738
  %9 = load i32* %index18, align 4, !dbg !738, !tbaa !559
  %idxprom19 = sext i32 %9 to i64, !dbg !738
  %arrayidx20 = getelementptr inbounds %struct.simple_bitmap_def** %src, i64 %idxprom19, !dbg !738
  %10 = load %struct.simple_bitmap_def** %arrayidx20, align 8, !dbg !738, !tbaa !578
  %arraydecay = getelementptr inbounds %struct.simple_bitmap_def* %10, i64 0, i32 3, i64 0, !dbg !738
  br label %for.body25, !dbg !734

for.body25:                                       ; preds = %for.body25, %for.body25.lr.ph
  %r.050 = phi i64* [ %arraydecay22, %for.body25.lr.ph ], [ %incdec.ptr26, %for.body25 ]
  %p.049 = phi i64* [ %arraydecay, %for.body25.lr.ph ], [ %incdec.ptr, %for.body25 ]
  %i.048 = phi i32 [ 0, %for.body25.lr.ph ], [ %inc, %for.body25 ]
  %incdec.ptr = getelementptr inbounds i64* %p.049, i64 1, !dbg !739
  tail call void @llvm.dbg.value(metadata !{i64* %incdec.ptr}, i64 0, metadata !420), !dbg !739
  %11 = load i64* %p.049, align 8, !dbg !739, !tbaa !596
  %incdec.ptr26 = getelementptr inbounds i64* %r.050, i64 1, !dbg !739
  tail call void @llvm.dbg.value(metadata !{i64* %incdec.ptr26}, i64 0, metadata !421), !dbg !739
  %12 = load i64* %r.050, align 8, !dbg !739, !tbaa !596
  %and = and i64 %12, %11, !dbg !739
  store i64 %and, i64* %r.050, align 8, !dbg !739, !tbaa !596
  %inc = add i32 %i.048, 1, !dbg !734
  tail call void @llvm.dbg.value(metadata !{i32 %inc}, i64 0, metadata !417), !dbg !734
  %exitcond = icmp eq i32 %inc, %2, !dbg !734
  br i1 %exitcond, label %for.cond10.backedge, label %for.body25, !dbg !734

if.end32:                                         ; preds = %for.cond10.preheader, %for.cond10.backedge, %if.then8
  ret void, !dbg !740
}
