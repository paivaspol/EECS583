  %arrayidx = getelementptr inbounds [1 x %struct.basic_block_def*]* %bb1, i64 0, i64 %idxprom, !dbg !744
  %1 = load %struct.basic_block_def** %arrayidx, align 8, !dbg !744, !tbaa !578
  tail call void @llvm.dbg.value(metadata !{%struct.basic_block_def* %1}, i64 0, metadata !427), !dbg !744
  %size = getelementptr inbounds %struct.simple_bitmap_def* %dst, i64 0, i32 1, !dbg !745
  %2 = load i32* %size, align 4, !dbg !745, !tbaa !559
  tail call void @llvm.dbg.value(metadata !{i32 %2}, i64 0, metadata !428), !dbg !745
  %succ = getelementptr inbounds %struct.basic_block_def* %1, i64 0, i32 5, !dbg !746
  %e.056 = load %struct.edge_def** %succ, align 8, !dbg !746
  %cond57 = icmp eq %struct.edge_def* %e.056, null, !dbg !746
  br i1 %cond57, label %if.then7, label %for.body, !dbg !746

for.cond:                                         ; preds = %for.body
  %succ_next = getelementptr inbounds %struct.edge_def* %e.058, i64 0, i32 1, !dbg !746
  %e.0 = load %struct.edge_def** %succ_next, align 8, !dbg !746
  %cond = icmp eq %struct.edge_def* %e.0, null, !dbg !746
  br i1 %cond, label %if.then7, label %for.body, !dbg !746

for.body:                                         ; preds = %entry, %for.cond
  %e.058 = phi %struct.edge_def* [ %e.0, %for.cond ], [ %e.056, %entry ]
  %dest = getelementptr inbounds %struct.edge_def* %e.058, i64 0, i32 3, !dbg !748
  %3 = load %struct.basic_block_def** %dest, align 8, !dbg !748, !tbaa !578
  %cmp2 = icmp eq %struct.basic_block_def* %3, getelementptr inbounds ([2 x %struct.basic_block_def]* @entry_exit_blocks, i64 0, i64 1), !dbg !748
  br i1 %cmp2, label %for.cond, label %for.cond9.preheader, !dbg !748

for.cond9.preheader:                              ; preds = %for.body
  %index = getelementptr inbounds %struct.basic_block_def* %3, i64 0, i32 11, !dbg !750
  %4 = load i32* %index, align 4, !dbg !750, !tbaa !559
  %idxprom4 = sext i32 %4 to i64, !dbg !750
  %arrayidx5 = getelementptr inbounds %struct.simple_bitmap_def** %src, i64 %idxprom4, !dbg !750
  %5 = load %struct.simple_bitmap_def** %arrayidx5, align 8, !dbg !750, !tbaa !578
  tail call void @llvm.dbg.value(metadata !{%struct.simple_bitmap_def* %dst}, i64 0, metadata !751) #2, !dbg !752
  tail call void @llvm.dbg.value(metadata !{%struct.simple_bitmap_def* %5}, i64 0, metadata !753) #2, !dbg !752
  %elms.i = getelementptr inbounds %struct.simple_bitmap_def* %dst, i64 0, i32 3, !dbg !754
  %6 = bitcast [1 x i64]* %elms.i to i8*, !dbg !754
  %elms1.i = getelementptr inbounds %struct.simple_bitmap_def* %5, i64 0, i32 3, !dbg !754
  %7 = bitcast [1 x i64]* %elms1.i to i8*, !dbg !754
  %conv.i = zext i32 %2 to i64, !dbg !754
  %mul.i = shl nuw nsw i64 %conv.i, 3, !dbg !754
  tail call void @llvm.memcpy.p0i8.p0i8.i64(i8* %6, i8* %7, i64 %mul.i, i32 8, i1 false) #2, !dbg !754
  %e.1.in51 = getelementptr inbounds %struct.edge_def* %e.058, i64 0, i32 1, !dbg !755
  %e.152 = load %struct.edge_def** %e.1.in51, align 8, !dbg !755
  %cmp1053 = icmp eq %struct.edge_def* %e.152, null, !dbg !755
  br i1 %cmp1053, label %if.end31, label %for.body11.lr.ph, !dbg !755

for.body11.lr.ph:                                 ; preds = %for.cond9.preheader
  %arraydecay21 = getelementptr inbounds %struct.simple_bitmap_def* %dst, i64 0, i32 3, i64 0, !dbg !756
