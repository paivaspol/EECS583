  br label %for.body11, !dbg !755

if.then7:                                         ; preds = %entry, %for.cond
  tail call void @llvm.dbg.value(metadata !{%struct.simple_bitmap_def* %dst}, i64 0, metadata !759) #2, !dbg !761
  %arraydecay.i = getelementptr inbounds %struct.simple_bitmap_def* %dst, i64 0, i32 3, i64 0, !dbg !762
  %8 = bitcast i64* %arraydecay.i to i8*, !dbg !762
