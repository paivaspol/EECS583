  %12 = load %struct.simple_bitmap_def** %arrayidx19, align 8, !dbg !764, !tbaa !578
  %arraydecay = getelementptr inbounds %struct.simple_bitmap_def* %12, i64 0, i32 3, i64 0, !dbg !764
  br label %for.body24, !dbg !757

for.body24:                                       ; preds = %for.body24, %for.body24.lr.ph
  %r.050 = phi i64* [ %arraydecay21, %for.body24.lr.ph ], [ %incdec.ptr25, %for.body24 ]
  %p.049 = phi i64* [ %arraydecay, %for.body24.lr.ph ], [ %incdec.ptr, %for.body24 ]
  %i.048 = phi i32 [ 0, %for.body24.lr.ph ], [ %inc, %for.body24 ]
  %incdec.ptr = getelementptr inbounds i64* %p.049, i64 1, !dbg !765
  tail call void @llvm.dbg.value(metadata !{i64* %incdec.ptr}, i64 0, metadata !433), !dbg !765
  %13 = load i64* %p.049, align 8, !dbg !765, !tbaa !596
  %incdec.ptr25 = getelementptr inbounds i64* %r.050, i64 1, !dbg !765
  tail call void @llvm.dbg.value(metadata !{i64* %incdec.ptr25}, i64 0, metadata !434), !dbg !765
  %14 = load i64* %r.050, align 8, !dbg !765, !tbaa !596
  %or = or i64 %14, %13, !dbg !765
  store i64 %or, i64* %r.050, align 8, !dbg !765, !tbaa !596
  %inc = add i32 %i.048, 1, !dbg !757
  tail call void @llvm.dbg.value(metadata !{i32 %inc}, i64 0, metadata !430), !dbg !757
  %exitcond = icmp eq i32 %inc, %2, !dbg !757
  br i1 %exitcond, label %for.cond9.backedge, label %for.body24, !dbg !757

