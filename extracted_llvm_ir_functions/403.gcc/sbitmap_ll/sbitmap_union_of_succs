  tail call void @llvm.memset.p0i8.i64(i8* %8, i8 0, i64 %conv.i46, i32 1, i1 false) #2, !dbg !762
  br label %if.end31, !dbg !760

for.body11:                                       ; preds = %for.body11.lr.ph, %for.cond9.backedge
  %e.154 = phi %struct.edge_def* [ %e.152, %for.body11.lr.ph ], [ %e.1, %for.cond9.backedge ]
  %dest12 = getelementptr inbounds %struct.edge_def* %e.154, i64 0, i32 3, !dbg !763
