for.body24.lr.ph:                                 ; preds = %for.body11
  %index17 = getelementptr inbounds %struct.basic_block_def* %10, i64 0, i32 11, !dbg !764
  %11 = load i32* %index17, align 4, !dbg !764, !tbaa !559
  %idxprom18 = sext i32 %11 to i64, !dbg !764
