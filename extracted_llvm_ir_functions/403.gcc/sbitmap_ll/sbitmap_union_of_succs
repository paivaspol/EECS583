  %cmp13 = icmp eq %struct.basic_block_def* %10, getelementptr inbounds ([2 x %struct.basic_block_def]* @entry_exit_blocks, i64 0, i64 1), !dbg !763
  %brmerge = or i1 %cmp13, %cmp2347, !dbg !763
  br i1 %brmerge, label %for.cond9.backedge, label %for.body24.lr.ph, !dbg !763

for.cond9.backedge:                               ; preds = %for.body11, %for.body24
  %e.1.in = getelementptr inbounds %struct.edge_def* %e.154, i64 0, i32 1, !dbg !755
  %e.1 = load %struct.edge_def** %e.1.in, align 8, !dbg !755
  %cmp10 = icmp eq %struct.edge_def* %e.1, null, !dbg !755
