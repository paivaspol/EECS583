define internal fastcc void @set_lang(%struct.cpp_reader* nocapture %pfile, i32 %lang) #0 {
entry:
  %idxprom = sext i32 %lang to i64
  %lang1 = getelementptr inbounds %struct.cpp_reader* %pfile, i64 0, i32 37, i32 11
  store i32 %lang, i32* %lang1, align 4, !tbaa !4
  %c99 = getelementptr inbounds [10 x %struct.lang_flags]* @lang_defaults, i64 0, i64 %idxprom, i32 0
  %0 = load i8* %c99, align 8, !tbaa !0
  %c993 = getelementptr inbounds %struct.cpp_reader* %pfile, i64 0, i32 37, i32 41
  store i8 %0, i8* %c993, align 1, !tbaa !0
  %objc = getelementptr inbounds [10 x %struct.lang_flags]* @lang_defaults, i64 0, i64 %idxprom, i32 1
  %1 = load i8* %objc, align 1, !tbaa !0
  %objc5 = getelementptr inbounds %struct.cpp_reader* %pfile, i64 0, i32 37, i32 16
  store i8 %1, i8* %objc5, align 1, !tbaa !0
  %cplusplus = getelementptr inbounds [10 x %struct.lang_flags]* @lang_defaults, i64 0, i64 %idxprom, i32 2
  %2 = load i8* %cplusplus, align 2, !tbaa !0
  %cplusplus7 = getelementptr inbounds %struct.cpp_reader* %pfile, i64 0, i32 37, i32 14
  store i8 %2, i8* %cplusplus7, align 1, !tbaa !0
  %extended_numbers = getelementptr inbounds [10 x %struct.lang_flags]* @lang_defaults, i64 0, i64 %idxprom, i32 3
  %3 = load i8* %extended_numbers, align 1, !tbaa !0
  %extended_numbers9 = getelementptr inbounds %struct.cpp_reader* %pfile, i64 0, i32 37, i32 20
  store i8 %3, i8* %extended_numbers9, align 1, !tbaa !0
  %trigraphs = getelementptr inbounds [10 x %struct.lang_flags]* @lang_defaults, i64 0, i64 %idxprom, i32 4
  %4 = load i8* %trigraphs, align 4, !tbaa !0
  %trigraphs11 = getelementptr inbounds %struct.cpp_reader* %pfile, i64 0, i32 37, i32 18
  store i8 %4, i8* %trigraphs11, align 1, !tbaa !0
  %dollars_in_ident = getelementptr inbounds [10 x %struct.lang_flags]* @lang_defaults, i64 0, i64 %idxprom, i32 5
  %5 = load i8* %dollars_in_ident, align 1, !tbaa !0
  %dollars_in_ident13 = getelementptr inbounds %struct.cpp_reader* %pfile, i64 0, i32 37, i32 39
  store i8 %5, i8* %dollars_in_ident13, align 1, !tbaa !0
  %cplusplus_comments = getelementptr inbounds [10 x %struct.lang_flags]* @lang_defaults, i64 0, i64 %idxprom, i32 6
  %6 = load i8* %cplusplus_comments, align 2, !tbaa !0
  %cplusplus_comments15 = getelementptr inbounds %struct.cpp_reader* %pfile, i64 0, i32 37, i32 15
  store i8 %6, i8* %cplusplus_comments15, align 1, !tbaa !0
  %digraphs = getelementptr inbounds [10 x %struct.lang_flags]* @lang_defaults, i64 0, i64 %idxprom, i32 7
  %7 = load i8* %digraphs, align 1, !tbaa !0
  %digraphs17 = getelementptr inbounds %struct.cpp_reader* %pfile, i64 0, i32 37, i32 19
  store i8 %7, i8* %digraphs17, align 1, !tbaa !0
  ret void
}
