define void @output_add_character(%struct.output_buffer* %buffer, i32 %c) #2 {
entry:
  %ideal_maximum_length.i = getelementptr inbounds %struct.output_buffer* %buffer, i64 0, i32 0, i32 2
  %0 = load i32* %ideal_maximum_length.i, align 4, !tbaa !0
  %cmp.i = icmp sgt i32 %0, 0
  br i1 %cmp.i, label %land.lhs.true, label %if.end

land.lhs.true:                                    ; preds = %entry
  %maximum_length.i = getelementptr inbounds %struct.output_buffer* %buffer, i64 0, i32 0, i32 1
  %1 = load i32* %maximum_length.i, align 4, !tbaa !0
  %line_length.i = getelementptr inbounds %struct.output_buffer* %buffer, i64 0, i32 3
  %2 = load i32* %line_length.i, align 4, !tbaa !0
  %sub.i = sub nsw i32 %1, %2
  %cmp = icmp slt i32 %sub.i, 1
  br i1 %cmp, label %if.then, label %if.end

if.then:                                          ; preds = %land.lhs.true
  tail call void @output_add_newline(%struct.output_buffer* %buffer) #11
  br label %if.end

if.end:                                           ; preds = %entry, %if.then, %land.lhs.true
  %next_free = getelementptr inbounds %struct.output_buffer* %buffer, i64 0, i32 2, i32 3
  %3 = load i8** %next_free, align 8, !tbaa !3
  %add.ptr = getelementptr inbounds i8* %3, i64 1
  %chunk_limit = getelementptr inbounds %struct.output_buffer* %buffer, i64 0, i32 2, i32 4
  %4 = load i8** %chunk_limit, align 8, !tbaa !3
  %cmp2 = icmp ugt i8* %add.ptr, %4
  br i1 %cmp2, label %if.then3, label %if.end4

if.then3:                                         ; preds = %if.end
  %obstack = getelementptr inbounds %struct.output_buffer* %buffer, i64 0, i32 2
  tail call void @_obstack_newchunk(%struct.obstack* %obstack, i32 1) #10
  %.pre = load i8** %next_free, align 8, !tbaa !3
  br label %if.end4

if.end4:                                          ; preds = %if.then3, %if.end
  %5 = phi i8* [ %.pre, %if.then3 ], [ %3, %if.end ]
  %conv = trunc i32 %c to i8
  %incdec.ptr = getelementptr inbounds i8* %5, i64 1
  store i8* %incdec.ptr, i8** %next_free, align 8, !tbaa !3
  store i8 %conv, i8* %5, align 1, !tbaa !1
  %line_length = getelementptr inbounds %struct.output_buffer* %buffer, i64 0, i32 3
  %6 = load i32* %line_length, align 4, !tbaa !0
  %inc = add nsw i32 %6, 1
  store i32 %inc, i32* %line_length, align 4, !tbaa !0
  ret void
}
