define void @output_printf(%struct.output_buffer* %buffer, i8* %msgid, ...) #2 {
entry:
  %str.i.i = alloca i8*, align 8
  %ap = alloca [1 x %struct.__va_list_tag], align 16
  %arraydecay1 = bitcast [1 x %struct.__va_list_tag]* %ap to i8*
  call void @llvm.va_start(i8* %arraydecay1)
  %format_args = getelementptr inbounds %struct.output_buffer* %buffer, i64 0, i32 0, i32 8
  %0 = load [1 x %struct.__va_list_tag]** %format_args, align 8, !tbaa !3
  store [1 x %struct.__va_list_tag]* %ap, [1 x %struct.__va_list_tag]** %format_args, align 8, !tbaa !3
  %arraydecay.i = getelementptr inbounds [1 x %struct.__va_list_tag]* %ap, i64 0, i64 0
  %1 = bitcast i8** %str.i.i to i8*
  call void @llvm.lifetime.start(i64 8, i8* %1) #3
  %call.i.i = call i32 @vasprintf(i8** %str.i.i, i8* %msgid, %struct.__va_list_tag* %arraydecay.i) #10
  %2 = load i8** %str.i.i, align 8, !tbaa !3
  call void @llvm.lifetime.end(i64 8, i8* %1) #3
  %call1.i = call i64 @strlen(i8* %2) #12
  %add.ptr.i = getelementptr inbounds i8* %2, i64 %call1.i
  call fastcc void @wrap_text(%struct.output_buffer* %buffer, i8* %2, i8* %add.ptr.i) #10
  call void @free(i8* %2) #10
  store [1 x %struct.__va_list_tag]* %0, [1 x %struct.__va_list_tag]** %format_args, align 8, !tbaa !3
  call void @llvm.va_end(i8* %arraydecay1)
  ret void
}
