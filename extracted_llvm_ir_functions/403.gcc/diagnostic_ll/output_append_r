define internal fastcc void @output_append_r(%struct.output_buffer* %buffer, i8* nocapture %start, i32 %length) #2 {
entry:
  %next_free = getelementptr inbounds %struct.output_buffer* %buffer, i64 0, i32 2, i32 3
  %0 = load i8** %next_free, align 8, !tbaa !3
  %idx.ext = sext i32 %length to i64
  %add.ptr = getelementptr inbounds i8* %0, i64 %idx.ext
  %chunk_limit = getelementptr inbounds %struct.output_buffer* %buffer, i64 0, i32 2, i32 4
  %1 = load i8** %chunk_limit, align 8, !tbaa !3
  %cmp = icmp ugt i8* %add.ptr, %1
  br i1 %cmp, label %if.then, label %if.end

if.then:                                          ; preds = %entry
  %obstack = getelementptr inbounds %struct.output_buffer* %buffer, i64 0, i32 2
  tail call void @_obstack_newchunk(%struct.obstack* %obstack, i32 %length) #10
  %.pre = load i8** %next_free, align 8, !tbaa !3
  br label %if.end

if.end:                                           ; preds = %if.then, %entry
  %2 = phi i8* [ %.pre, %if.then ], [ %0, %entry ]
  tail call void @llvm.memcpy.p0i8.p0i8.i64(i8* %2, i8* %start, i64 %idx.ext, i32 1, i1 false)
  %3 = load i8** %next_free, align 8, !tbaa !3
  %add.ptr4 = getelementptr inbounds i8* %3, i64 %idx.ext
  store i8* %add.ptr4, i8** %next_free, align 8, !tbaa !3
  %line_length = getelementptr inbounds %struct.output_buffer* %buffer, i64 0, i32 3
  %4 = load i32* %line_length, align 4, !tbaa !0
  %add = add nsw i32 %4, %length
  store i32 %add, i32* %line_length, align 4, !tbaa !0
  ret void
}
