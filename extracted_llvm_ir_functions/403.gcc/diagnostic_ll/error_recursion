define internal fastcc void @error_recursion() #7 {
entry:
  %0 = load i32* @diagnostic_lock, align 4, !tbaa !0
  %cmp = icmp slt i32 %0, 3
  br i1 %cmp, label %if.then, label %if.end

if.then:                                          ; preds = %entry
  %1 = load %struct.diagnostic_context** @global_dc, align 8, !tbaa !3
  %2 = getelementptr inbounds %struct.diagnostic_context* %1, i64 0, i32 0
  tail call fastcc void @diagnostic_finish(%struct.output_buffer* %2) #11
  br label %if.end

if.end:                                           ; preds = %if.then, %entry
  %3 = load %struct._IO_FILE** @stderr, align 8, !tbaa !3
  tail call void (%struct._IO_FILE*, i8*, ...)* @fnotice(%struct._IO_FILE* %3, i8* getelementptr inbounds ([63 x i8]* @.str19, i64 0, i64 0)) #11
  %4 = load %struct._IO_FILE** @stderr, align 8, !tbaa !3
  tail call void (%struct._IO_FILE*, i8*, ...)* @fnotice(%struct._IO_FILE* %4, i8* getelementptr inbounds ([421 x i8]* @.str12, i64 0, i64 0), i8* getelementptr inbounds ([27 x i8]* @.str13, i64 0, i64 0)) #11
  tail call void @exit(i32 1) #14
  unreachable
}
