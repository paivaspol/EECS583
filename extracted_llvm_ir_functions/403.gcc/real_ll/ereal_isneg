define i32 @ereal_isneg(%struct.realvaluetype* byval nocapture align 8 %x) #0 {
entry:
  %0 = bitcast %struct.realvaluetype* %x to i8*
  %ex.sroa.1.10.idx = getelementptr inbounds i8* %0, i64 10
  %ex.sroa.1.10.cast = bitcast i8* %ex.sroa.1.10.idx to i16*
  %ex.sroa.1.10.copyload = load i16* %ex.sroa.1.10.cast, align 2
  %.lobit.i = lshr i16 %ex.sroa.1.10.copyload, 15
  %1 = zext i16 %.lobit.i to i32
  ret i32 %1
}
