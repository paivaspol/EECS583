  %cmp2.i = icmp slt i32 %inc.i, 5, !dbg !2306
  br i1 %cmp2.i, label %for.body.i, label %if.end, !dbg !2306

if.then:                                          ; preds = %for.body.i
  %4 = bitcast %struct.realvaluetype* %agg.result to i8*, !dbg !2308
  call void @llvm.memcpy.p0i8.p0i8.i64(i8* %4, i8* %1, i64 24, i32 8, i1 false), !dbg !2308, !tbaa.struct !1565
  br label %return, !dbg !2308

if.end:                                           ; preds = %for.inc.i, %entry
  %r.sroa.01112 = bitcast [3 x i64]* %r.sroa.0 to i16*
  call fastcc void @eldexp(i16* %arraydecay, i32 %n, i16* %r.sroa.01112) #7, !dbg !2309
  %r.sroa.0.0.cast3 = bitcast [3 x i64]* %r.sroa.0 to i8*, !dbg !2310
  %r.sroa.0.12.raw_idx = getelementptr inbounds i8* %r.sroa.0.0.cast3, i64 12, !dbg !2310
  call void @llvm.memset.p0i8.i64(i8* %r.sroa.0.12.raw_idx, i8 0, i64 12, i32 4, i1 false), !dbg !2310
  %r.sroa.0.0.cast4 = bitcast %struct.realvaluetype* %agg.result to i8*, !dbg !2312
  call void @llvm.memcpy.p0i8.p0i8.i64(i8* %r.sroa.0.0.cast4, i8* %r.sroa.0.0.cast3, i64 24, i32 8, i1 false), !dbg !2312
  br label %return, !dbg !2312

return:                                           ; preds = %if.end, %if.then
  ret void, !dbg !2313
}
