  call void @llvm.memcpy.p0i8.p0i8.i64(i8* %0, i8* %1, i64 12, i32 2, i1 false), !dbg !2300
  %arraydecay = getelementptr inbounds [6 x i16]* %e, i64 0, i64 0, !dbg !2301
  tail call void @llvm.dbg.value(metadata !{i16* %arraydecay}, i64 0, metadata !2302), !dbg !2303
  %arrayidx.i = getelementptr inbounds [6 x i16]* %e, i64 0, i64 5, !dbg !2304
  %2 = load i16* %arrayidx.i, align 2, !dbg !2304, !tbaa !1057
  %and.i = and i16 %2, 32767, !dbg !2304
  %cmp.i = icmp eq i16 %and.i, 32767, !dbg !2304
  br i1 %cmp.i, label %for.body.i, label %if.end, !dbg !2304

for.body.i:                                       ; preds = %entry, %for.inc.i
  %i.012.i = phi i32 [ %inc.i, %for.inc.i ], [ 0, %entry ]
  %x.addr.011.i = phi i16* [ %incdec.ptr.i, %for.inc.i ], [ %arraydecay, %entry ]
  %3 = load i16* %x.addr.011.i, align 2, !dbg !2305, !tbaa !1057
  %cmp5.i = icmp eq i16 %3, 0, !dbg !2305
  br i1 %cmp5.i, label %for.inc.i, label %if.then, !dbg !2305

for.inc.i:                                        ; preds = %for.body.i
  %incdec.ptr.i = getelementptr inbounds i16* %x.addr.011.i, i64 1, !dbg !2305
  %inc.i = add nsw i32 %i.012.i, 1, !dbg !2306
