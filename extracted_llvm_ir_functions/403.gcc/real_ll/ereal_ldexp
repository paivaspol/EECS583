  call void @llvm.dbg.value(metadata !{i32 %n}, i64 0, metadata !373), !dbg !2297
  call void @llvm.dbg.declare(metadata !{[6 x i16]* %e}, metadata !374), !dbg !2298
  call void @llvm.dbg.declare(metadata !1553, metadata !376), !dbg !2299
  %0 = bitcast [6 x i16]* %e to i8*, !dbg !2300
