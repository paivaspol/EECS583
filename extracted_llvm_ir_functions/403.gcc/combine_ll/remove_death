define %struct.rtx_def* @remove_death(i32 %regno, %struct.rtx_def* %insn) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{i32 %regno}, i64 0, metadata !500), !dbg !2657
  tail call void @llvm.dbg.value(metadata !{%struct.rtx_def* %insn}, i64 0, metadata !501), !dbg !2658
  %call = tail call %struct.rtx_def* @find_regno_note(%struct.rtx_def* %insn, i32 1, i32 %regno) #7, !dbg !2659
  tail call void @llvm.dbg.value(metadata !{%struct.rtx_def* %call}, i64 0, metadata !502), !dbg !2659
  %tobool = icmp eq %struct.rtx_def* %call, null, !dbg !2660
  br i1 %tobool, label %if.end, label %if.then, !dbg !2660

if.then:                                          ; preds = %entry
  %idxprom = zext i32 %regno to i64, !dbg !2661
  %0 = load %struct.varray_head_tag** @reg_n_info, align 8, !dbg !2661, !tbaa !1685
  %data = getelementptr inbounds %struct.varray_head_tag* %0, i64 0, i32 4, !dbg !2661
  %reg = bitcast %union.varray_data_tag* %data to [1 x %struct.reg_info_def*]*, !dbg !2661
  %arrayidx = getelementptr inbounds [1 x %struct.reg_info_def*]* %reg, i64 0, i64 %idxprom, !dbg !2661
  %1 = load %struct.reg_info_def** %arrayidx, align 8, !dbg !2661, !tbaa !1685
  %deaths = getelementptr inbounds %struct.reg_info_def* %1, i64 0, i32 6, !dbg !2661
  %2 = load i32* %deaths, align 4, !dbg !2661, !tbaa !1676
  %dec = add nsw i32 %2, -1, !dbg !2661
  store i32 %dec, i32* %deaths, align 4, !dbg !2661, !tbaa !1676
  tail call void @remove_note(%struct.rtx_def* %insn, %struct.rtx_def* %call) #7, !dbg !2663
  br label %if.end, !dbg !2664

if.end:                                           ; preds = %entry, %if.then
  ret %struct.rtx_def* %call, !dbg !2665
}
