define i32 @flow_loop_outside_edge_p(%struct.loop* nocapture %loop, %struct.edge_def* nocapture %e) #0 {
entry:
  %dest = getelementptr inbounds %struct.edge_def* %e, i64 0, i32 3
  %0 = load %struct.basic_block_def** %dest, align 8, !tbaa !0
  %header = getelementptr inbounds %struct.loop* %loop, i64 0, i32 1
  %1 = load %struct.basic_block_def** %header, align 8, !tbaa !0
  %cmp = icmp eq %struct.basic_block_def* %0, %1
  br i1 %cmp, label %if.end, label %if.then

if.then:                                          ; preds = %entry
  tail call void @fancy_abort(i8* getelementptr inbounds ([51 x i8]* @.str17, i64 0, i64 0), i32 832, i8* getelementptr inbounds ([25 x i8]* @__FUNCTION__.flow_loop_outside_edge_p, i64 0, i64 0)) #7
  unreachable

if.end:                                           ; preds = %entry
  %src = getelementptr inbounds %struct.edge_def* %e, i64 0, i32 2
  %2 = load %struct.basic_block_def** %src, align 8, !tbaa !0
  %cmp1 = icmp eq %struct.basic_block_def* %2, getelementptr inbounds ([2 x %struct.basic_block_def]* @entry_exit_blocks, i64 0, i64 0)
  br i1 %cmp1, label %lor.end, label %lor.rhs

lor.rhs:                                          ; preds = %if.end
  %index = getelementptr inbounds %struct.basic_block_def* %2, i64 0, i32 11
  %3 = load i32* %index, align 4, !tbaa !3
  %div = lshr i32 %3, 6
  %idxprom = zext i32 %div to i64
  %nodes = getelementptr inbounds %struct.loop* %loop, i64 0, i32 8
  %4 = load %struct.simple_bitmap_def** %nodes, align 8, !tbaa !0
  %arrayidx = getelementptr inbounds %struct.simple_bitmap_def* %4, i64 0, i32 3, i64 %idxprom
  %5 = load i64* %arrayidx, align 8, !tbaa !4
  %rem = and i32 %3, 63
  %sh_prom = zext i32 %rem to i64
  %6 = shl i64 1, %sh_prom
  %and9 = and i64 %6, %5
  %lnot = icmp eq i64 %and9, 0
  br label %lor.end

lor.end:                                          ; preds = %lor.rhs, %if.end
  %7 = phi i1 [ true, %if.end ], [ %lnot, %lor.rhs ]
  %lor.ext = zext i1 %7 to i32
  ret i32 %lor.ext
}
