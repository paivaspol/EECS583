define i32 @flow_loops_update(%struct.loops* nocapture %loops, i32 %flags) #0 {
entry:
  %array = getelementptr inbounds %struct.loops* %loops, i64 0, i32 2
  %0 = load %struct.loop** %array, align 8, !tbaa !0
  %tobool = icmp eq %struct.loop* %0, null
  br i1 %tobool, label %if.end, label %if.then

if.then:                                          ; preds = %entry
  tail call void @flow_loops_free(%struct.loops* %loops) #6
  br label %if.end

if.end:                                           ; preds = %entry, %if.then
  %call = tail call i32 @flow_loops_find(%struct.loops* %loops, i32 %flags) #6
  ret i32 %call
}
