define void @c_expand_decl_stmt(%union.tree_node* nocapture %t) #0 {
entry:
  %operands = getelementptr inbounds %union.tree_node* %t, i64 0, i32 0, i32 2
  %arrayidx = bitcast i32* %operands to %union.tree_node**
  %0 = load %union.tree_node** %arrayidx, align 8, !tbaa !0
  %code = getelementptr inbounds %union.tree_node* %0, i64 0, i32 0, i32 0, i32 2
  %1 = bitcast [4 x i8]* %code to i32*
  %bf.load = load i32* %1, align 8
  %bf.clear = and i32 %bf.load, 255
  %cmp = icmp eq i32 %bf.clear, 30
  br i1 %cmp, label %land.lhs.true, label %if.end

land.lhs.true:                                    ; preds = %entry
  %context = getelementptr inbounds %union.tree_node* %0, i64 0, i32 0, i32 9
  %2 = load %union.tree_node** %context, align 8, !tbaa !0
  %3 = load %union.tree_node** @current_function_decl, align 8, !tbaa !0
  %cmp2 = icmp eq %union.tree_node* %2, %3
  br i1 %cmp2, label %land.lhs.true3, label %if.end

land.lhs.true3:                                   ; preds = %land.lhs.true
  %saved_tree = getelementptr inbounds %union.tree_node* %0, i64 0, i32 0, i32 20
  %4 = load %union.tree_node** %saved_tree, align 8, !tbaa !0
  %tobool = icmp eq %union.tree_node* %4, null
  br i1 %tobool, label %if.end, label %if.then

if.then:                                          ; preds = %land.lhs.true3
  tail call fastcc void @c_expand_body(%union.tree_node* %0, i32 1, i32 0) #10
  br label %if.end

if.end:                                           ; preds = %land.lhs.true3, %if.then, %land.lhs.true, %entry
  ret void
}
