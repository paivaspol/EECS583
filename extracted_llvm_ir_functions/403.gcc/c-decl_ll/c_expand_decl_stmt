define void @c_expand_decl_stmt(%union.tree_node* nocapture %t) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %t}, i64 0, metadata !1875), !dbg !5338
  %operands = getelementptr inbounds %union.tree_node* %t, i64 0, i32 0, i32 2, !dbg !5339
  %arrayidx = bitcast i32* %operands to %union.tree_node**, !dbg !5339
  %0 = load %union.tree_node** %arrayidx, align 8, !dbg !5339, !tbaa !2183
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %0}, i64 0, metadata !1876), !dbg !5339
  %code = getelementptr inbounds %union.tree_node* %0, i64 0, i32 0, i32 0, i32 2, !dbg !5340
  %1 = bitcast [4 x i8]* %code to i32*, !dbg !5340
  %bf.load = load i32* %1, align 8, !dbg !5340
  %bf.clear = and i32 %bf.load, 255, !dbg !5340
  %cmp = icmp eq i32 %bf.clear, 30, !dbg !5340
  br i1 %cmp, label %land.lhs.true, label %if.end, !dbg !5340

land.lhs.true:                                    ; preds = %entry
  %context = getelementptr inbounds %union.tree_node* %0, i64 0, i32 0, i32 9, !dbg !5340
  %2 = load %union.tree_node** %context, align 8, !dbg !5340, !tbaa !2183
  %3 = load %union.tree_node** @current_function_decl, align 8, !dbg !5340, !tbaa !2183
  %cmp2 = icmp eq %union.tree_node* %2, %3, !dbg !5340
  br i1 %cmp2, label %land.lhs.true3, label %if.end, !dbg !5340

land.lhs.true3:                                   ; preds = %land.lhs.true
  %saved_tree = getelementptr inbounds %union.tree_node* %0, i64 0, i32 0, i32 20, !dbg !5340
  %4 = load %union.tree_node** %saved_tree, align 8, !dbg !5340, !tbaa !2183
  %tobool = icmp eq %union.tree_node* %4, null, !dbg !5340
  br i1 %tobool, label %if.end, label %if.then, !dbg !5340

if.then:                                          ; preds = %land.lhs.true3
  tail call fastcc void @c_expand_body(%union.tree_node* %0, i32 1, i32 0) #11, !dbg !5341
  br label %if.end, !dbg !5341

if.end:                                           ; preds = %land.lhs.true3, %if.then, %land.lhs.true, %entry
  ret void, !dbg !5342
}
