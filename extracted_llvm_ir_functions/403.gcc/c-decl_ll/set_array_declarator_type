define %union.tree_node* @set_array_declarator_type(%union.tree_node* %decl, %union.tree_node* %type, i32 %abstract_p) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %decl}, i64 0, metadata !1507), !dbg !3575
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %type}, i64 0, metadata !1508), !dbg !3576
  tail call void @llvm.dbg.value(metadata !{i32 %abstract_p}, i64 0, metadata !1509), !dbg !3577
  %operands = getelementptr inbounds %union.tree_node* %decl, i64 0, i32 0, i32 2, !dbg !3578
  %arrayidx = bitcast i32* %operands to %union.tree_node**, !dbg !3578
  store %union.tree_node* %type, %union.tree_node** %arrayidx, align 8, !dbg !3578, !tbaa !2183
  %tobool = icmp eq i32 %abstract_p, 0, !dbg !3579
  br i1 %tobool, label %if.end, label %land.lhs.true, !dbg !3579

land.lhs.true:                                    ; preds = %entry
  %type1 = getelementptr inbounds %union.tree_node* %decl, i64 0, i32 0, i32 0, i32 1, !dbg !3579
  %0 = load %union.tree_node** %type1, align 8, !dbg !3579, !tbaa !2183
  %cmp = icmp eq %union.tree_node* %0, null, !dbg !3579
  br i1 %cmp, label %lor.lhs.false, label %if.then, !dbg !3579

lor.lhs.false:                                    ; preds = %land.lhs.true
  %static_flag = getelementptr inbounds %union.tree_node* %decl, i64 0, i32 0, i32 0, i32 2, !dbg !3579
  %1 = bitcast [4 x i8]* %static_flag to i32*, !dbg !3579
  %bf.load = load i32* %1, align 8, !dbg !3579
  %bf.clear = and i32 %bf.load, 262144, !dbg !3579
  %tobool3 = icmp eq i32 %bf.clear, 0, !dbg !3579
  br i1 %tobool3, label %if.end, label %if.then, !dbg !3579

if.then:                                          ; preds = %lor.lhs.false, %land.lhs.true
  tail call void (i8*, ...)* @error(i8* getelementptr inbounds ([49 x i8]* @.str168, i64 0, i64 0)) #9, !dbg !3580
  br label %if.end, !dbg !3580

if.end:                                           ; preds = %lor.lhs.false, %entry, %if.then
  ret %union.tree_node* %decl, !dbg !3581
}
