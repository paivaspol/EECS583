define void @pending_xref_error() #0 {
entry:
  %0 = load %union.tree_node** @pending_invalid_xref, align 8, !tbaa !0
  %cmp = icmp eq %union.tree_node* %0, null
  br i1 %cmp, label %if.end, label %if.then

if.then:                                          ; preds = %entry
  %1 = load i8** @pending_invalid_xref_file, align 8, !tbaa !0
  %2 = load i32* @pending_invalid_xref_line, align 4, !tbaa !3
  %id = getelementptr inbounds %union.tree_node* %0, i64 0, i32 0, i32 1
  %3 = getelementptr inbounds i8** %id, i64 1
  %4 = load i8** %3, align 8, !tbaa !0
  tail call void (i8*, i32, i8*, ...)* @error_with_file_and_line(i8* %1, i32 %2, i8* getelementptr inbounds ([34 x i8]* @.str155, i64 0, i64 0), i8* %4) #8
  br label %if.end

if.end:                                           ; preds = %entry, %if.then
  store %union.tree_node* null, %union.tree_node** @pending_invalid_xref, align 8, !tbaa !0
  ret void
}
