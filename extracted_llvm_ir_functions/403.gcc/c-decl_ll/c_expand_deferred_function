define void @c_expand_deferred_function(%union.tree_node* %fndecl) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %fndecl}, i64 0, metadata !1752), !dbg !5215
  %inline_flag = getelementptr inbounds %union.tree_node* %fndecl, i64 0, i32 0, i32 5, !dbg !5216
  %0 = bitcast [8 x i8]* %inline_flag to i64*, !dbg !5216
  %bf.load = load i64* %0, align 8, !dbg !5216
  %bf.cast = and i64 %bf.load, 2048, !dbg !5216
  %tobool = icmp eq i64 %bf.cast, 0, !dbg !5216
  br i1 %tobool, label %if.end, label %land.lhs.true, !dbg !5216

land.lhs.true:                                    ; preds = %entry
  %result = getelementptr inbounds %union.tree_node* %fndecl, i64 0, i32 0, i32 11, !dbg !5216
  %1 = load %union.tree_node** %result, align 8, !dbg !5216, !tbaa !2183
  %tobool2 = icmp eq %union.tree_node* %1, null, !dbg !5216
  br i1 %tobool2, label %if.end, label %if.then, !dbg !5216

if.then:                                          ; preds = %land.lhs.true
  tail call fastcc void @c_expand_body(%union.tree_node* %fndecl, i32 0, i32 0) #11, !dbg !5217
  store %union.tree_node* null, %union.tree_node** @current_function_decl, align 8, !dbg !5219, !tbaa !2183
  br label %if.end, !dbg !5220

if.end:                                           ; preds = %land.lhs.true, %entry, %if.then
  ret void, !dbg !5221
}
