define void @cpp_error_from_errno(%struct.cpp_reader* %pfile, i8* %name) #0 {
entry:
  %call = tail call i32* @__errno_location() #7
  %0 = load i32* %call, align 4, !tbaa !3
  %call1 = tail call i8* @xstrerror(i32 %0) #6
  tail call void (%struct.cpp_reader*, i8*, ...)* @cpp_error(%struct.cpp_reader* %pfile, i8* getelementptr inbounds ([7 x i8]* @.str2, i64 0, i64 0), i8* %name, i8* %call1) #5
  ret void
}
