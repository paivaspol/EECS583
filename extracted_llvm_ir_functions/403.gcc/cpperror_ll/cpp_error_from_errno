define void @cpp_error_from_errno(%struct.cpp_reader* %pfile, i8* %name) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{%struct.cpp_reader* %pfile}, i64 0, metadata !539), !dbg !697
  tail call void @llvm.dbg.value(metadata !{i8* %name}, i64 0, metadata !540), !dbg !698
  %call = tail call i32* @__errno_location() #8, !dbg !699
  %0 = load i32* %call, align 4, !dbg !699, !tbaa !603
  %call1 = tail call i8* @xstrerror(i32 %0) #7, !dbg !699
  tail call void (%struct.cpp_reader*, i8*, ...)* @cpp_error(%struct.cpp_reader* %pfile, i8* getelementptr inbounds ([7 x i8]* @.str2, i64 0, i64 0), i8* %name, i8* %call1) #6, !dbg !699
  ret void, !dbg !700
}
