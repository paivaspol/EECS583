define void @cpp_error_with_line(%struct.cpp_reader* %pfile, i32 %line, i32 %column, i8* nocapture %msgid, ...) #0 {
entry:
  %ap = alloca [1 x %struct.__va_list_tag], align 16
  call void @llvm.dbg.value(metadata !{%struct.cpp_reader* %pfile}, i64 0, metadata !531), !dbg !689
  call void @llvm.dbg.value(metadata !{i32 %line}, i64 0, metadata !532), !dbg !689
  call void @llvm.dbg.value(metadata !{i32 %column}, i64 0, metadata !533), !dbg !689
  call void @llvm.dbg.value(metadata !{i8* %msgid}, i64 0, metadata !534), !dbg !689
  call void @llvm.dbg.declare(metadata !{[1 x %struct.__va_list_tag]* %ap}, metadata !535), !dbg !690
  %arraydecay1 = bitcast [1 x %struct.__va_list_tag]* %ap to i8*, !dbg !690
  call void @llvm.va_start(i8* %arraydecay1), !dbg !690
  %call = call i32 @_cpp_begin_message(%struct.cpp_reader* %pfile, i32 3, i32 %line, i32 %column) #6, !dbg !691
  %tobool = icmp eq i32 %call, 0, !dbg !691
  br i1 %tobool, label %if.end, label %do.body, !dbg !691

do.body:                                          ; preds = %entry
  %arraydecay = getelementptr inbounds [1 x %struct.__va_list_tag]* %ap, i64 0, i64 0, !dbg !690
  %0 = load %struct._IO_FILE** @stderr, align 8, !dbg !693, !tbaa !596
  %call3 = call i32 @vfprintf(%struct._IO_FILE* %0, i8* %msgid, %struct.__va_list_tag* %arraydecay) #7, !dbg !693
  %1 = load %struct._IO_FILE** @stderr, align 8, !dbg !693, !tbaa !596
  %call4 = call i32 @_IO_putc(i32 10, %struct._IO_FILE* %1) #7, !dbg !693
  br label %if.end, !dbg !693

if.end:                                           ; preds = %entry, %do.body
  call void @llvm.va_end(i8* %arraydecay1), !dbg !695
  ret void, !dbg !696
}
