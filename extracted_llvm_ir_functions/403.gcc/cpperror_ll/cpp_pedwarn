define void @cpp_pedwarn(%struct.cpp_reader* %pfile, i8* nocapture %msgid, ...) #0 {
entry:
  %ap = alloca [1 x %struct.__va_list_tag], align 16
  call void @llvm.dbg.value(metadata !{%struct.cpp_reader* %pfile}, i64 0, metadata !557), !dbg !717
  call void @llvm.dbg.value(metadata !{i8* %msgid}, i64 0, metadata !558), !dbg !717
  call void @llvm.dbg.declare(metadata !{[1 x %struct.__va_list_tag]* %ap}, metadata !559), !dbg !718
  %arraydecay1 = bitcast [1 x %struct.__va_list_tag]* %ap to i8*, !dbg !718
  call void @llvm.va_start(i8* %arraydecay1), !dbg !718
  %call = call i32 @_cpp_begin_message(%struct.cpp_reader* %pfile, i32 2, i32 0, i32 0) #6, !dbg !719
  %tobool = icmp eq i32 %call, 0, !dbg !719
  br i1 %tobool, label %if.end, label %do.body, !dbg !719

do.body:                                          ; preds = %entry
  %arraydecay = getelementptr inbounds [1 x %struct.__va_list_tag]* %ap, i64 0, i64 0, !dbg !718
  %0 = load %struct._IO_FILE** @stderr, align 8, !dbg !721, !tbaa !596
  %call3 = call i32 @vfprintf(%struct._IO_FILE* %0, i8* %msgid, %struct.__va_list_tag* %arraydecay) #7, !dbg !721
  %1 = load %struct._IO_FILE** @stderr, align 8, !dbg !721, !tbaa !596
  %call4 = call i32 @_IO_putc(i32 10, %struct._IO_FILE* %1) #7, !dbg !721
  br label %if.end, !dbg !721

if.end:                                           ; preds = %entry, %do.body
  call void @llvm.va_end(i8* %arraydecay1), !dbg !723
  ret void, !dbg !724
}
