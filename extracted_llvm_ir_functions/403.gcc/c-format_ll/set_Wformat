define void @set_Wformat(i32 %setting) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{i32 %setting}, i64 0, metadata !404), !dbg !1244
  store i32 %setting, i32* @warn_format, align 4, !dbg !1245, !tbaa !1246
  store i32 %setting, i32* @warn_format_y2k, align 4, !dbg !1249, !tbaa !1246
  store i32 %setting, i32* @warn_format_extra_args, align 4, !dbg !1250, !tbaa !1246
  %cmp = icmp eq i32 %setting, 1, !dbg !1251
  br i1 %cmp, label %if.end, label %if.then, !dbg !1251

if.then:                                          ; preds = %entry
  store i32 %setting, i32* @warn_format_nonliteral, align 4, !dbg !1252, !tbaa !1246
  store i32 %setting, i32* @warn_format_security, align 4, !dbg !1254, !tbaa !1246
  br label %if.end, !dbg !1255

if.end:                                           ; preds = %entry, %if.then
  ret void, !dbg !1256
}
