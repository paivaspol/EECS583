define internal void @ggc_mark_tree_ptr(i8* nocapture %elt) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{i8* %elt}, i64 0, metadata !1174), !dbg !1405
  %0 = bitcast i8* %elt to %union.tree_node**, !dbg !1406
  %1 = load %union.tree_node** %0, align 8, !dbg !1406, !tbaa !1351
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %1}, i64 0, metadata !1175), !dbg !1406
  %cmp = icmp eq %union.tree_node* %1, null, !dbg !1406
  br i1 %cmp, label %do.end8, label %land.lhs.true, !dbg !1406

land.lhs.true:                                    ; preds = %entry
  %2 = bitcast %union.tree_node* %1 to i8*, !dbg !1406
  %call = tail call i32 @ggc_set_mark(i8* %2) #6, !dbg !1406
  %tobool = icmp eq i32 %call, 0, !dbg !1406
  br i1 %tobool, label %do.body1, label %do.end8, !dbg !1406

do.body1:                                         ; preds = %land.lhs.true
  %3 = load %struct.varray_head_tag** @ggc_pending_trees, align 8, !dbg !1407, !tbaa !1351
  %elements_used = getelementptr inbounds %struct.varray_head_tag* %3, i64 0, i32 1, !dbg !1407
  %4 = load i64* %elements_used, align 8, !dbg !1407, !tbaa !1409
  %num_elements = getelementptr inbounds %struct.varray_head_tag* %3, i64 0, i32 0, !dbg !1407
  %5 = load i64* %num_elements, align 8, !dbg !1407, !tbaa !1409
  %cmp2 = icmp ult i64 %4, %5, !dbg !1407
  br i1 %cmp2, label %if.end, label %if.then3, !dbg !1407

if.then3:                                         ; preds = %do.body1
  %mul = shl i64 %5, 1, !dbg !1407
  %call5 = tail call %struct.varray_head_tag* @varray_grow(%struct.varray_head_tag* %3, i64 %mul) #6, !dbg !1407
  store %struct.varray_head_tag* %call5, %struct.varray_head_tag** @ggc_pending_trees, align 8, !dbg !1407, !tbaa !1351
  %elements_used6.phi.trans.insert = getelementptr inbounds %struct.varray_head_tag* %call5, i64 0, i32 1
  %.pre = load i64* %elements_used6.phi.trans.insert, align 8, !dbg !1407, !tbaa !1409
  br label %if.end, !dbg !1407

if.end:                                           ; preds = %do.body1, %if.then3
  %6 = phi i64 [ %4, %do.body1 ], [ %.pre, %if.then3 ]
  %7 = phi %struct.varray_head_tag* [ %3, %do.body1 ], [ %call5, %if.then3 ]
  %elements_used6 = getelementptr inbounds %struct.varray_head_tag* %7, i64 0, i32 1, !dbg !1407
  %inc = add i64 %6, 1, !dbg !1407
  store i64 %inc, i64* %elements_used6, align 8, !dbg !1407, !tbaa !1409
  %data = getelementptr inbounds %struct.varray_head_tag* %7, i64 0, i32 4, !dbg !1407
  %tree = bitcast %union.varray_data_tag* %data to [1 x %union.tree_node*]*, !dbg !1407
  %arrayidx = getelementptr inbounds [1 x %union.tree_node*]* %tree, i64 0, i64 %6, !dbg !1407
  store %union.tree_node* %1, %union.tree_node** %arrayidx, align 8, !dbg !1407, !tbaa !1351
  br label %do.end8, !dbg !1407

do.end8:                                          ; preds = %land.lhs.true, %entry, %if.end
  ret void, !dbg !1410
}
