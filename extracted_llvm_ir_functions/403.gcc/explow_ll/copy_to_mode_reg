define %struct.rtx_def* @copy_to_mode_reg(i32 %mode, %struct.rtx_def* %x) #2 {
entry:
  tail call void @llvm.dbg.value(metadata !{i32 %mode}, i64 0, metadata !1769), !dbg !2143
  tail call void @llvm.dbg.value(metadata !{%struct.rtx_def* %x}, i64 0, metadata !1770), !dbg !2144
  %call = tail call %struct.rtx_def* @gen_reg_rtx(i32 %mode) #5, !dbg !2145
  tail call void @llvm.dbg.value(metadata !{%struct.rtx_def* %call}, i64 0, metadata !1771), !dbg !2145
  %call1 = tail call i32 @general_operand(%struct.rtx_def* %x, i32 0) #5, !dbg !2146
  %tobool = icmp eq i32 %call1, 0, !dbg !2146
  br i1 %tobool, label %if.then, label %if.end, !dbg !2146

if.then:                                          ; preds = %entry
  %call2 = tail call %struct.rtx_def* @force_operand(%struct.rtx_def* %x, %struct.rtx_def* %call) #5, !dbg !2147
  tail call void @llvm.dbg.value(metadata !{%struct.rtx_def* %call2}, i64 0, metadata !1770), !dbg !2147
  br label %if.end, !dbg !2147

if.end:                                           ; preds = %entry, %if.then
  %x.addr.0 = phi %struct.rtx_def* [ %x, %entry ], [ %call2, %if.then ]
  %0 = bitcast %struct.rtx_def* %x.addr.0 to i32*, !dbg !2148
  %bf.load = load i32* %0, align 8, !dbg !2148
  %bf.lshr = lshr i32 %bf.load, 16, !dbg !2148
  %bf.clear = and i32 %bf.lshr, 255, !dbg !2148
  %cmp = icmp eq i32 %bf.clear, %mode, !dbg !2148
  %cmp6 = icmp eq i32 %bf.clear, 0, !dbg !2148
  %or.cond = or i1 %cmp, %cmp6, !dbg !2148
  br i1 %or.cond, label %if.end8, label %if.then7, !dbg !2148

if.then7:                                         ; preds = %if.end
  tail call void @fancy_abort(i8* getelementptr inbounds ([50 x i8]* @.str, i64 0, i64 0), i32 713, i8* getelementptr inbounds ([17 x i8]* @__FUNCTION__.copy_to_mode_reg, i64 0, i64 0)) #7, !dbg !2149
  unreachable, !dbg !2149

if.end8:                                          ; preds = %if.end
  %cmp9 = icmp eq %struct.rtx_def* %x.addr.0, %call, !dbg !2150
  br i1 %cmp9, label %if.end12, label %if.then10, !dbg !2150

if.then10:                                        ; preds = %if.end8
  %call11 = tail call %struct.rtx_def* @emit_move_insn(%struct.rtx_def* %call, %struct.rtx_def* %x.addr.0) #5, !dbg !2151
  br label %if.end12, !dbg !2151

if.end12:                                         ; preds = %if.end8, %if.then10
  ret %struct.rtx_def* %call, !dbg !2152
}
