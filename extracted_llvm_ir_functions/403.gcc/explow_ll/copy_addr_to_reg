entry:
  tail call void @llvm.dbg.value(metadata !{%struct.rtx_def* %x}, i64 0, metadata !1766), !dbg !2141
  %0 = load i32* @target_flags, align 4, !dbg !2142, !tbaa !2023
  %and = lshr i32 %0, 25, !dbg !2142
  %1 = and i32 %and, 1, !dbg !2142
  %2 = or i32 %1, 4, !dbg !2142
  %call = tail call %struct.rtx_def* @copy_to_mode_reg(i32 %2, %struct.rtx_def* %x) #6, !dbg !2142
