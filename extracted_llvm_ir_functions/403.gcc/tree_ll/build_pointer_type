define %union.tree_node* @build_pointer_type(%union.tree_node* %to_type) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %to_type}, i64 0, metadata !1551), !dbg !2816
  %0 = getelementptr inbounds %union.tree_node* %to_type, i64 0, i32 0, i32 8, !dbg !2817
  %1 = load %union.tree_node** %0, align 8, !dbg !2817, !tbaa !1913
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %1}, i64 0, metadata !1552), !dbg !2817
  %cmp = icmp eq %union.tree_node* %1, null, !dbg !2818
  br i1 %cmp, label %if.end, label %return, !dbg !2818

if.end:                                           ; preds = %entry
  %call = tail call %union.tree_node* @make_node(i32 13) #11, !dbg !2819
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %call}, i64 0, metadata !1552), !dbg !2819
  %type1 = getelementptr inbounds %union.tree_node* %call, i64 0, i32 0, i32 0, i32 1, !dbg !2820
  store %union.tree_node* %to_type, %union.tree_node** %type1, align 8, !dbg !2820, !tbaa !1913
  store %union.tree_node* %call, %union.tree_node** %0, align 8, !dbg !2821, !tbaa !1913
  tail call void @layout_type(%union.tree_node* %call) #10, !dbg !2822
  br label %return, !dbg !2823

return:                                           ; preds = %entry, %if.end
  %retval.0 = phi %union.tree_node* [ %call, %if.end ], [ %1, %entry ]
  ret %union.tree_node* %retval.0, !dbg !2824
}
