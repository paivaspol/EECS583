define %union.tree_node* @unsave_expr_now(%union.tree_node* %expr) #0 {
entry:
  %expr.addr = alloca %union.tree_node*, align 8
  store %union.tree_node* %expr, %union.tree_node** %expr.addr, align 8, !tbaa !0
  %0 = load void (%union.tree_node**)** @lang_unsave, align 8, !tbaa !0
  %cmp = icmp eq void (%union.tree_node**)* %0, null
  br i1 %cmp, label %if.else, label %if.then

if.then:                                          ; preds = %entry
  call void %0(%union.tree_node** %expr.addr) #9
  br label %if.end

if.else:                                          ; preds = %entry
  call fastcc void @unsave_expr_now_r(%union.tree_node* %expr) #10
  br label %if.end

if.end:                                           ; preds = %if.else, %if.then
  %1 = load %union.tree_node** %expr.addr, align 8, !tbaa !0
  ret %union.tree_node* %1
}
