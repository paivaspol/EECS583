define %union.tree_node* @build_offset_type(%union.tree_node* %basetype, %union.tree_node* %type) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %basetype}, i64 0, metadata !1602), !dbg !3332
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %type}, i64 0, metadata !1603), !dbg !3332
  %call = tail call %union.tree_node* @make_node(i32 14) #11, !dbg !3333
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %call}, i64 0, metadata !1604), !dbg !3333
  %0 = getelementptr inbounds %union.tree_node* %basetype, i64 0, i32 0, i32 15, !dbg !3334
  %1 = load %union.tree_node** %0, align 8, !dbg !3334, !tbaa !1913
  %2 = getelementptr inbounds %union.tree_node* %call, i64 0, i32 0, i32 13, !dbg !3334
  store %union.tree_node* %1, %union.tree_node** %2, align 8, !dbg !3334, !tbaa !1913
  %type3 = getelementptr inbounds %union.tree_node* %call, i64 0, i32 0, i32 0, i32 1, !dbg !3335
  store %union.tree_node* %type, %union.tree_node** %type3, align 8, !dbg !3335, !tbaa !1913
  %3 = ptrtoint %union.tree_node* %basetype to i64, !dbg !3336
  %and = and i64 %3, 262143, !dbg !3336
  %4 = ptrtoint %union.tree_node* %type to i64, !dbg !3336
  %and4 = and i64 %4, 262143, !dbg !3336
  %add = add i64 %and4, %and, !dbg !3336
  %conv = trunc i64 %add to i32, !dbg !3336
  tail call void @llvm.dbg.value(metadata !{i32 %conv}, i64 0, metadata !1605), !dbg !3336
  %call5 = tail call %union.tree_node* @type_hash_canon(i32 %conv, %union.tree_node* %call) #11, !dbg !3337
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %call5}, i64 0, metadata !1604), !dbg !3337
  %size = getelementptr inbounds %union.tree_node* %call5, i64 0, i32 0, i32 2, !dbg !3338
  %5 = bitcast i32* %size to %union.tree_node**, !dbg !3338
  %6 = load %union.tree_node** %5, align 8, !dbg !3338, !tbaa !1913
  %cmp = icmp eq %union.tree_node* %6, null, !dbg !3338
  br i1 %cmp, label %if.then, label %if.end, !dbg !3338

if.then:                                          ; preds = %entry
  tail call void @layout_type(%union.tree_node* %call5) #10, !dbg !3339
  br label %if.end, !dbg !3339

if.end:                                           ; preds = %entry, %if.then
  ret %union.tree_node* %call5, !dbg !3340
}
