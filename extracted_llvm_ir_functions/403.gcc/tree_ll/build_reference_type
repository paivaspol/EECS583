define %union.tree_node* @build_reference_type(%union.tree_node* %to_type) #0 {
entry:
  %0 = getelementptr inbounds %union.tree_node* %to_type, i64 0, i32 0, i32 9
  %1 = load %union.tree_node** %0, align 8, !tbaa !0
  %tobool = icmp eq %union.tree_node* %1, null
  br i1 %tobool, label %if.end, label %return

if.end:                                           ; preds = %entry
  %call = tail call %union.tree_node* @make_node(i32 15) #10
  %type1 = getelementptr inbounds %union.tree_node* %call, i64 0, i32 0, i32 0, i32 1
  store %union.tree_node* %to_type, %union.tree_node** %type1, align 8, !tbaa !0
  store %union.tree_node* %call, %union.tree_node** %0, align 8, !tbaa !0
  tail call void @layout_type(%union.tree_node* %call) #9
  br label %return

return:                                           ; preds = %entry, %if.end
  %retval.0 = phi %union.tree_node* [ %call, %if.end ], [ %1, %entry ]
  ret %union.tree_node* %retval.0
}
