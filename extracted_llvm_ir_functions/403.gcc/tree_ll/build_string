entry:
  %call = tail call %union.tree_node* @make_node(i32 29) #10
  %0 = getelementptr inbounds %union.tree_node* %call, i64 0, i32 0, i32 2
  store i32 %len, i32* %0, align 4, !tbaa !4
  %call1 = tail call i8* @ggc_alloc_string(i8* %str, i32 %len) #9
  %pointer = getelementptr inbounds %union.tree_node* %call, i64 0, i32 0, i32 4
