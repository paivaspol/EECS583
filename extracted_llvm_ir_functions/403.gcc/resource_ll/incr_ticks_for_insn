define void @incr_ticks_for_insn(%struct.rtx_def* %insn) #0 {
entry:
  %0 = load %struct.param_info** @compiler_params, align 8, !tbaa !0
  %value = getelementptr inbounds %struct.param_info* %0, i64 2, i32 1
  %1 = load i32* %value, align 4, !tbaa !4
  %call = tail call fastcc i32 @find_basic_block(%struct.rtx_def* %insn, i32 %1) #7
  %cmp = icmp eq i32 %call, -1
  br i1 %cmp, label %if.end, label %if.then

if.then:                                          ; preds = %entry
  %idxprom = sext i32 %call to i64
  %2 = load i32** @bb_ticks, align 8, !tbaa !0
  %arrayidx1 = getelementptr inbounds i32* %2, i64 %idxprom
  %3 = load i32* %arrayidx1, align 4, !tbaa !4
  %inc = add nsw i32 %3, 1
  store i32 %inc, i32* %arrayidx1, align 4, !tbaa !4
  br label %if.end

if.end:                                           ; preds = %entry, %if.then
  ret void
}
