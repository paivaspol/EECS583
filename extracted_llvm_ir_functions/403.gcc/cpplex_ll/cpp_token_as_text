define i8* @cpp_token_as_text(%struct.cpp_reader* %pfile, %struct.cpp_token* nocapture %token) #2 {
entry:
  tail call void @llvm.dbg.value(metadata !{%struct.cpp_reader* %pfile}, i64 0, metadata !584), !dbg !1993
  tail call void @llvm.dbg.value(metadata !{%struct.cpp_token* %token}, i64 0, metadata !585), !dbg !1994
  tail call void @llvm.dbg.value(metadata !{%struct.cpp_token* %token}, i64 0, metadata !1995), !dbg !1997
  %type.i = getelementptr inbounds %struct.cpp_token* %token, i64 0, i32 2, !dbg !1998
  %bf.load.i = load i8* %type.i, align 2, !dbg !1998
  %idxprom.i = zext i8 %bf.load.i to i64, !dbg !1998
  %category.i = getelementptr inbounds [68 x %struct.token_spelling]* @token_spellings, i64 0, i64 %idxprom.i, i32 0, !dbg !1998
  %0 = load i32* %category.i, align 16, !dbg !1998, !tbaa !1950
  switch i32 %0, label %cpp_token_len.exit [
    i32 3, label %sw.bb.i
    i32 4, label %sw.bb.i
    i32 2, label %sw.bb2.i
  ], !dbg !1998

sw.bb.i:                                          ; preds = %entry, %entry
  %len1.i = getelementptr inbounds %struct.cpp_token* %token, i64 0, i32 4, i32 0, i32 0, !dbg !1999
  %1 = load i32* %len1.i, align 4, !dbg !1999, !tbaa !1019
  tail call void @llvm.dbg.value(metadata !{i32 %1}, i64 0, metadata !2000), !dbg !1999
  br label %cpp_token_len.exit, !dbg !1999

sw.bb2.i:                                         ; preds = %entry
  %val3.i = getelementptr inbounds %struct.cpp_token* %token, i64 0, i32 4, !dbg !2001
  %node.i = bitcast %union.anon* %val3.i to %struct.cpp_hashnode**, !dbg !2001
  %2 = load %struct.cpp_hashnode** %node.i, align 8, !dbg !2001, !tbaa !995
  %len4.i = getelementptr inbounds %struct.cpp_hashnode* %2, i64 0, i32 0, i32 0, !dbg !2001
  %3 = load i32* %len4.i, align 4, !dbg !2001, !tbaa !1019
  tail call void @llvm.dbg.value(metadata !{i32 %3}, i64 0, metadata !2000), !dbg !2001
  br label %cpp_token_len.exit, !dbg !2001

cpp_token_len.exit:                               ; preds = %entry, %sw.bb.i, %sw.bb2.i
  %len.0.i = phi i32 [ %3, %sw.bb2.i ], [ %1, %sw.bb.i ], [ 0, %entry ]
  %add.i = add i32 %len.0.i, 5, !dbg !2002
  tail call void @llvm.dbg.value(metadata !{i32 %add.i}, i64 0, metadata !586), !dbg !1996
  %conv = zext i32 %add.i to i64, !dbg !2003
  %call1 = tail call i8* @_cpp_unaligned_alloc(%struct.cpp_reader* %pfile, i64 %conv) #11, !dbg !2003
  tail call void @llvm.dbg.value(metadata !{i8* %call1}, i64 0, metadata !587), !dbg !2003
  %call2 = tail call i8* @cpp_spell_token(%struct.cpp_reader* %pfile, %struct.cpp_token* %token, i8* %call1) #11, !dbg !2004
  tail call void @llvm.dbg.value(metadata !{i8* %call2}, i64 0, metadata !588), !dbg !2004
  store i8 0, i8* %call2, align 1, !dbg !2005, !tbaa !996
  ret i8* %call1, !dbg !2006
}
