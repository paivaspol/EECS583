define i8* @_cpp_aligned_alloc(%struct.cpp_reader* nocapture %pfile, i64 %len) #2 {
entry:
  tail call void @llvm.dbg.value(metadata !{%struct.cpp_reader* %pfile}, i64 0, metadata !778), !dbg !2320
  tail call void @llvm.dbg.value(metadata !{i64 %len}, i64 0, metadata !779), !dbg !2321
  %a_buff = getelementptr inbounds %struct.cpp_reader* %pfile, i64 0, i32 6, !dbg !2322
  %0 = load %struct._cpp_buff** %a_buff, align 8, !dbg !2322, !tbaa !995
  tail call void @llvm.dbg.value(metadata !{%struct._cpp_buff* %0}, i64 0, metadata !780), !dbg !2322
  %cur = getelementptr inbounds %struct._cpp_buff* %0, i64 0, i32 2, !dbg !2323
  %1 = load i8** %cur, align 8, !dbg !2323, !tbaa !995
  tail call void @llvm.dbg.value(metadata !{i8* %1}, i64 0, metadata !781), !dbg !2323
  %limit = getelementptr inbounds %struct._cpp_buff* %0, i64 0, i32 3, !dbg !2324
  %2 = load i8** %limit, align 8, !dbg !2324, !tbaa !995
  %sub.ptr.lhs.cast = ptrtoint i8* %2 to i64, !dbg !2324
  %sub.ptr.rhs.cast = ptrtoint i8* %1 to i64, !dbg !2324
  %sub.ptr.sub = sub i64 %sub.ptr.lhs.cast, %sub.ptr.rhs.cast, !dbg !2324
  %cmp = icmp ult i64 %sub.ptr.sub, %len, !dbg !2324
  br i1 %cmp, label %if.then, label %if.end, !dbg !2324

if.then:                                          ; preds = %entry
  %call = tail call %struct._cpp_buff* @_cpp_get_buff(%struct.cpp_reader* %pfile, i64 %len) #11, !dbg !2325
  tail call void @llvm.dbg.value(metadata !{%struct._cpp_buff* %call}, i64 0, metadata !780), !dbg !2325
  %3 = load %struct._cpp_buff** %a_buff, align 8, !dbg !2327, !tbaa !995
  %next = getelementptr inbounds %struct._cpp_buff* %call, i64 0, i32 0, !dbg !2327
  store %struct._cpp_buff* %3, %struct._cpp_buff** %next, align 8, !dbg !2327, !tbaa !995
  store %struct._cpp_buff* %call, %struct._cpp_buff** %a_buff, align 8, !dbg !2328, !tbaa !995
  %cur3 = getelementptr inbounds %struct._cpp_buff* %call, i64 0, i32 2, !dbg !2329
  %4 = load i8** %cur3, align 8, !dbg !2329, !tbaa !995
  tail call void @llvm.dbg.value(metadata !{i8* %4}, i64 0, metadata !781), !dbg !2329
  br label %if.end, !dbg !2330

if.end:                                           ; preds = %if.then, %entry
  %buff.0 = phi %struct._cpp_buff* [ %call, %if.then ], [ %0, %entry ]
  %result.0 = phi i8* [ %4, %if.then ], [ %1, %entry ]
  %add.ptr = getelementptr inbounds i8* %result.0, i64 %len, !dbg !2331
  %cur4 = getelementptr inbounds %struct._cpp_buff* %buff.0, i64 0, i32 2, !dbg !2331
  store i8* %add.ptr, i8** %cur4, align 8, !dbg !2331, !tbaa !995
  ret i8* %result.0, !dbg !2332
}
