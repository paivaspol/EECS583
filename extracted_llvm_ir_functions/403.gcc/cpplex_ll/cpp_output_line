define void @cpp_output_line(%struct.cpp_reader* %pfile, %struct._IO_FILE* nocapture %fp) #2 {
entry:
  tail call void @llvm.dbg.value(metadata !{%struct.cpp_reader* %pfile}, i64 0, metadata !679), !dbg !2100
  tail call void @llvm.dbg.value(metadata !{%struct._IO_FILE* %fp}, i64 0, metadata !680), !dbg !2101
  %call = tail call %struct.cpp_token* @cpp_get_token(%struct.cpp_reader* %pfile) #10, !dbg !2102
  tail call void @llvm.dbg.value(metadata !{%struct.cpp_token* %call}, i64 0, metadata !681), !dbg !2102
  %type9 = getelementptr inbounds %struct.cpp_token* %call, i64 0, i32 2, !dbg !2103
  %bf.load10 = load i8* %type9, align 2, !dbg !2103
  %cmp11 = icmp eq i8 %bf.load10, 67, !dbg !2103
  br i1 %cmp11, label %while.end, label %while.body, !dbg !2103

while.body:                                       ; preds = %entry, %while.cond.backedge
  %token.012 = phi %struct.cpp_token* [ %call1, %while.cond.backedge ], [ %call, %entry ]
  tail call void @cpp_output_token(%struct.cpp_token* %token.012, %struct._IO_FILE* %fp) #11, !dbg !2104
  %call1 = tail call %struct.cpp_token* @cpp_get_token(%struct.cpp_reader* %pfile) #10, !dbg !2106
  tail call void @llvm.dbg.value(metadata !{%struct.cpp_token* %call1}, i64 0, metadata !681), !dbg !2106
  %flags = getelementptr inbounds %struct.cpp_token* %call1, i64 0, i32 3, !dbg !2107
  %0 = load i8* %flags, align 1, !dbg !2107, !tbaa !996
  %and = and i8 %0, 1, !dbg !2107
  %tobool = icmp eq i8 %and, 0, !dbg !2107
  br i1 %tobool, label %while.cond.backedge, label %if.then, !dbg !2107

if.then:                                          ; preds = %while.body
  %call2 = tail call i32 @_IO_putc(i32 32, %struct._IO_FILE* %fp) #10, !dbg !2108
  br label %while.cond.backedge, !dbg !2108

while.cond.backedge:                              ; preds = %if.then, %while.body
  %type = getelementptr inbounds %struct.cpp_token* %call1, i64 0, i32 2, !dbg !2103
  %bf.load = load i8* %type, align 2, !dbg !2103
  %cmp = icmp eq i8 %bf.load, 67, !dbg !2103
  br i1 %cmp, label %while.end, label %while.body, !dbg !2103

while.end:                                        ; preds = %while.cond.backedge, %entry
  %call3 = tail call i32 @_IO_putc(i32 10, %struct._IO_FILE* %fp) #10, !dbg !2109
  ret void, !dbg !2110
}
