define %struct._cpp_buff* @_cpp_append_extend_buff(%struct.cpp_reader* nocapture %pfile, %struct._cpp_buff* nocapture %buff, i64 %min_extra) #2 {
entry:
  tail call void @llvm.dbg.value(metadata !{%struct.cpp_reader* %pfile}, i64 0, metadata !747), !dbg !2295
  tail call void @llvm.dbg.value(metadata !{%struct._cpp_buff* %buff}, i64 0, metadata !748), !dbg !2296
  tail call void @llvm.dbg.value(metadata !{i64 %min_extra}, i64 0, metadata !749), !dbg !2297
  %limit = getelementptr inbounds %struct._cpp_buff* %buff, i64 0, i32 3, !dbg !2298
  %0 = load i8** %limit, align 8, !dbg !2298, !tbaa !995
  %cur = getelementptr inbounds %struct._cpp_buff* %buff, i64 0, i32 2, !dbg !2298
  %1 = load i8** %cur, align 8, !dbg !2298, !tbaa !995
  %sub.ptr.lhs.cast = ptrtoint i8* %0 to i64, !dbg !2298
  %sub.ptr.rhs.cast = ptrtoint i8* %1 to i64, !dbg !2298
  %sub.ptr.sub = sub i64 %sub.ptr.lhs.cast, %sub.ptr.rhs.cast, !dbg !2298
  %mul = shl i64 %sub.ptr.sub, 1, !dbg !2298
  %add = add i64 %mul, %min_extra, !dbg !2298
  tail call void @llvm.dbg.value(metadata !{i64 %add}, i64 0, metadata !750), !dbg !2298
  %call = tail call %struct._cpp_buff* @_cpp_get_buff(%struct.cpp_reader* %pfile, i64 %add) #11, !dbg !2299
  tail call void @llvm.dbg.value(metadata !{%struct._cpp_buff* %call}, i64 0, metadata !751), !dbg !2299
  %next = getelementptr inbounds %struct._cpp_buff* %buff, i64 0, i32 0, !dbg !2300
  store %struct._cpp_buff* %call, %struct._cpp_buff** %next, align 8, !dbg !2300, !tbaa !995
  %base = getelementptr inbounds %struct._cpp_buff* %call, i64 0, i32 1, !dbg !2301
  %2 = load i8** %base, align 8, !dbg !2301, !tbaa !995
  %3 = load i8** %cur, align 8, !dbg !2301, !tbaa !995
  %4 = load i8** %limit, align 8, !dbg !2301, !tbaa !995
  %sub.ptr.lhs.cast4 = ptrtoint i8* %4 to i64, !dbg !2301
  %sub.ptr.rhs.cast5 = ptrtoint i8* %3 to i64, !dbg !2301
  %sub.ptr.sub6 = sub i64 %sub.ptr.lhs.cast4, %sub.ptr.rhs.cast5, !dbg !2301
