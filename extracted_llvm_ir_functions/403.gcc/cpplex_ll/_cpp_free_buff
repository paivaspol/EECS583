define void @_cpp_free_buff(%struct._cpp_buff* %buff) #1 {
entry:
  %tobool4 = icmp eq %struct._cpp_buff* %buff, null
  br i1 %tobool4, label %for.end, label %for.body

for.body:                                         ; preds = %entry, %for.body
  %buff.addr.05 = phi %struct._cpp_buff* [ %0, %for.body ], [ %buff, %entry ]
  %next1 = getelementptr inbounds %struct._cpp_buff* %buff.addr.05, i64 0, i32 0
  %0 = load %struct._cpp_buff** %next1, align 8, !tbaa !0
  %base = getelementptr inbounds %struct._cpp_buff* %buff.addr.05, i64 0, i32 1
  %1 = load i8** %base, align 8, !tbaa !0
  tail call void @free(i8* %1) #9
  %tobool = icmp eq %struct._cpp_buff* %0, null
  br i1 %tobool, label %for.end, label %for.body

for.end:                                          ; preds = %for.body, %entry
  ret void
}
