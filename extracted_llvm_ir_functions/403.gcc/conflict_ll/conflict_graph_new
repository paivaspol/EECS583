define %struct.conflict_graph_def* @conflict_graph_new(i32 %num_regs) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{i32 %num_regs}, i64 0, metadata !361), !dbg !715
  %call = tail call noalias i8* @xmalloc(i64 112) #7, !dbg !716
  %0 = bitcast i8* %call to %struct.conflict_graph_def*, !dbg !716
  tail call void @llvm.dbg.value(metadata !{%struct.conflict_graph_def* %0}, i64 0, metadata !362), !dbg !716
  %num_regs1 = getelementptr inbounds i8* %call, i64 8, !dbg !717
  %1 = bitcast i8* %num_regs1 to i32*, !dbg !717
  store i32 %num_regs, i32* %1, align 4, !dbg !717, !tbaa !718
  %call2 = tail call %struct.htab* @htab_create(i64 64, i32 (i8*)* @arc_hash, i32 (i8*, i8*)* @arc_eq, void (i8*)* null) #7, !dbg !721
  %arc_hash_table = bitcast i8* %call to %struct.htab**, !dbg !721
  store %struct.htab* %call2, %struct.htab** %arc_hash_table, align 8, !dbg !721, !tbaa !722
  %arc_obstack = getelementptr inbounds i8* %call, i64 24, !dbg !723
  %2 = bitcast i8* %arc_obstack to %struct.obstack*, !dbg !723
  %call3 = tail call i32 @_obstack_begin(%struct.obstack* %2, i32 0, i32 0, i8* (i64)* @xmalloc, void (i8*)* @free) #7, !dbg !723
  %conv = sext i32 %num_regs to i64, !dbg !724
  %mul = shl nsw i64 %conv, 3, !dbg !724
  %call4 = tail call noalias i8* @xmalloc(i64 %mul) #7, !dbg !724
  %3 = bitcast i8* %call4 to %struct.conflict_graph_arc_def**, !dbg !724
  %neighbor_heads = getelementptr inbounds i8* %call, i64 16, !dbg !724
  %4 = bitcast i8* %neighbor_heads to %struct.conflict_graph_arc_def***, !dbg !724
  store %struct.conflict_graph_arc_def** %3, %struct.conflict_graph_arc_def*** %4, align 8, !dbg !724, !tbaa !722
  tail call void @llvm.memset.p0i8.i64(i8* %call4, i8 0, i64 %mul, i32 8, i1 false), !dbg !725
  ret %struct.conflict_graph_def* %0, !dbg !726
}
