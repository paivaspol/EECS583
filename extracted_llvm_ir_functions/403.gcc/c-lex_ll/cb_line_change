define internal void @cb_line_change(%struct.cpp_reader* nocapture %pfile, %struct.cpp_token* nocapture %token, i32 %parsing_args) #0 {
entry:
  %line = getelementptr inbounds %struct.cpp_token* %token, i64 0, i32 0
  %0 = load i32* %line, align 4, !tbaa !3
  %1 = load %struct.line_map** @map, align 8, !tbaa !0
  %to_line = getelementptr inbounds %struct.line_map* %1, i64 0, i32 1
  %2 = load i32* %to_line, align 4, !tbaa !3
  %add = add i32 %2, %0
  %from_line = getelementptr inbounds %struct.line_map* %1, i64 0, i32 2
  %3 = load i32* %from_line, align 4, !tbaa !3
  %sub = sub i32 %add, %3
  store i32 %sub, i32* @src_lineno, align 4, !tbaa !3
  ret void
}
