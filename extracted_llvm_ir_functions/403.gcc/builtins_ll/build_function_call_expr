define internal fastcc %union.tree_node* @build_function_call_expr(%union.tree_node* %fn, %union.tree_node* %arglist) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %fn}, i64 0, metadata !2152), !dbg !4286
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %arglist}, i64 0, metadata !2153), !dbg !4286
  %type = getelementptr inbounds %union.tree_node* %fn, i64 0, i32 0, i32 0, i32 1, !dbg !4287
  %0 = load %union.tree_node** %type, align 8, !dbg !4287, !tbaa !2691
  %call = tail call %union.tree_node* @build_pointer_type(%union.tree_node* %0) #8, !dbg !4287
  %call1 = tail call %union.tree_node* @build1(i32 121, %union.tree_node* %call, %union.tree_node* %fn) #8, !dbg !4287
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %call1}, i64 0, metadata !2154), !dbg !4287
  %1 = load %union.tree_node** %type, align 8, !dbg !4288, !tbaa !2691
  %type5 = getelementptr inbounds %union.tree_node* %1, i64 0, i32 0, i32 0, i32 1, !dbg !4288
  %2 = load %union.tree_node** %type5, align 8, !dbg !4288, !tbaa !2691
  %call6 = tail call %union.tree_node* (i32, %union.tree_node*, ...)* @build(i32 53, %union.tree_node* %2, %union.tree_node* %call1, %union.tree_node* %arglist) #8, !dbg !4288
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %call6}, i64 0, metadata !2154), !dbg !4288
  %side_effects_flag = getelementptr inbounds %union.tree_node* %call6, i64 0, i32 0, i32 0, i32 2, !dbg !4289
  %3 = bitcast [4 x i8]* %side_effects_flag to i32*, !dbg !4289
  %bf.load = load i32* %3, align 8, !dbg !4289
  %bf.set = or i32 %bf.load, 256, !dbg !4289
  store i32 %bf.set, i32* %3, align 8, !dbg !4289
  %call8 = tail call %union.tree_node* @fold(%union.tree_node* %call6) #8, !dbg !4290
  ret %union.tree_node* %call8, !dbg !4290
}
