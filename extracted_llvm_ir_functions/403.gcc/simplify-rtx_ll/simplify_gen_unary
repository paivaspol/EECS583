define %struct.rtx_def* @simplify_gen_unary(i32 %code, i32 %mode, %struct.rtx_def* %op, i32 %op_mode) #0 {
entry:
  %call = tail call %struct.rtx_def* @simplify_unary_operation(i32 %code, i32 %mode, %struct.rtx_def* %op, i32 %op_mode) #5
  %cmp = icmp eq %struct.rtx_def* %call, null
  br i1 %cmp, label %if.end, label %return

if.end:                                           ; preds = %entry
  %call1 = tail call %struct.rtx_def* @gen_rtx_fmt_e(i32 %code, i32 %mode, %struct.rtx_def* %op) #4
  br label %return

return:                                           ; preds = %entry, %if.end
  %retval.0 = phi %struct.rtx_def* [ %call1, %if.end ], [ %call, %entry ]
  ret %struct.rtx_def* %retval.0
}
