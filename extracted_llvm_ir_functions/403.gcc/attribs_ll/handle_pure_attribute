define internal noalias %union.tree_node* @handle_pure_attribute(%union.tree_node** nocapture %node, %union.tree_node* nocapture %name, %union.tree_node* nocapture %args, i32 %flags, i8* nocapture %no_add_attrs) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{%union.tree_node** %node}, i64 0, metadata !830), !dbg !1546
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %name}, i64 0, metadata !831), !dbg !1547
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %args}, i64 0, metadata !832), !dbg !1548
  tail call void @llvm.dbg.value(metadata !{i32 %flags}, i64 0, metadata !833), !dbg !1549
  tail call void @llvm.dbg.value(metadata !{i8* %no_add_attrs}, i64 0, metadata !834), !dbg !1550
  %0 = load %union.tree_node** %node, align 8, !dbg !1551, !tbaa !1026
  %code = getelementptr inbounds %union.tree_node* %0, i64 0, i32 0, i32 0, i32 2, !dbg !1551
  %1 = bitcast [4 x i8]* %code to i32*, !dbg !1551
  %bf.load = load i32* %1, align 8, !dbg !1551
  %bf.clear = and i32 %bf.load, 255, !dbg !1551
  %cmp = icmp eq i32 %bf.clear, 30, !dbg !1551
  br i1 %cmp, label %if.then, label %if.else, !dbg !1551

if.then:                                          ; preds = %entry
  %pure_flag = getelementptr inbounds %union.tree_node* %0, i64 0, i32 0, i32 5, !dbg !1552
  %2 = bitcast [8 x i8]* %pure_flag to i64*, !dbg !1552
  %bf.load1 = load i64* %2, align 8, !dbg !1552
  %bf.set = or i64 %bf.load1, 2147483648, !dbg !1552
  store i64 %bf.set, i64* %2, align 8, !dbg !1552
  br label %if.end, !dbg !1552

if.else:                                          ; preds = %entry
  %id = getelementptr inbounds %union.tree_node* %name, i64 0, i32 0, i32 1, !dbg !1553
  %3 = getelementptr inbounds i8** %id, i64 1, !dbg !1553
  %4 = load i8** %3, align 8, !dbg !1553, !tbaa !1026
  tail call void (i8*, ...)* @warning(i8* getelementptr inbounds ([23 x i8]* @.str4, i64 0, i64 0), i8* %4) #5, !dbg !1553
  store i8 1, i8* %no_add_attrs, align 1, !dbg !1555, !tbaa !1050
  br label %if.end

if.end:                                           ; preds = %if.else, %if.then
  ret %union.tree_node* null, !dbg !1556
}
