define void @pedwarn_c99(i8* %msgid, ...) #0 {
entry:
  %dc = alloca %struct.diagnostic_context, align 8
  %ap = alloca [1 x %struct.__va_list_tag], align 16
  %0 = bitcast %struct.diagnostic_context* %dc to i8*
  call void @llvm.lifetime.start(i64 376, i8* %0) #1
  %arraydecay1 = bitcast [1 x %struct.__va_list_tag]* %ap to i8*
  call void @llvm.va_start(i8* %arraydecay1)
  %1 = load i8** @input_filename, align 8, !tbaa !0
  %2 = load i32* @lineno, align 4, !tbaa !3
  %3 = load i32* @flag_isoc99, align 4, !tbaa !3
  %tobool = icmp eq i32 %3, 0
  br i1 %tobool, label %lor.end, label %lor.rhs

lor.rhs:                                          ; preds = %entry
  %4 = load i32* @flag_pedantic_errors, align 4, !tbaa !3
  %lnot = icmp eq i32 %4, 0
  br label %lor.end

lor.end:                                          ; preds = %entry, %lor.rhs
  %5 = phi i1 [ true, %entry ], [ %lnot, %lor.rhs ]
  %lor.ext = zext i1 %5 to i32
  call void @set_diagnostic_context(%struct.diagnostic_context* %dc, i8* %msgid, [1 x %struct.__va_list_tag]* %ap, i8* %1, i32 %2, i32 %lor.ext) #3
  call void @report_diagnostic(%struct.diagnostic_context* %dc) #3
  call void @llvm.va_end(i8* %arraydecay1)
  call void @llvm.lifetime.end(i64 376, i8* %0) #1
  ret void
}
