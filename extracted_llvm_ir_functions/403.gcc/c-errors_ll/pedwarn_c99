define void @pedwarn_c99(i8* %msgid, ...) #0 {
entry:
  %dc = alloca %struct.diagnostic_context, align 8
  %ap = alloca [1 x %struct.__va_list_tag], align 16
  call void @llvm.dbg.value(metadata !{i8* %msgid}, i64 0, metadata !19), !dbg !176
  %0 = bitcast %struct.diagnostic_context* %dc to i8*, !dbg !177
  call void @llvm.lifetime.start(i64 376, i8* %0) #2, !dbg !177
  call void @llvm.dbg.declare(metadata !{%struct.diagnostic_context* %dc}, metadata !20), !dbg !177
  call void @llvm.dbg.declare(metadata !{[1 x %struct.__va_list_tag]* %ap}, metadata !174), !dbg !178
  %arraydecay1 = bitcast [1 x %struct.__va_list_tag]* %ap to i8*, !dbg !178
  call void @llvm.va_start(i8* %arraydecay1), !dbg !178
  %1 = load i8** @input_filename, align 8, !dbg !179, !tbaa !181
  %2 = load i32* @lineno, align 4, !dbg !179, !tbaa !184
  %3 = load i32* @flag_isoc99, align 4, !dbg !179, !tbaa !184
  %tobool = icmp eq i32 %3, 0, !dbg !179
  br i1 %tobool, label %lor.end, label %lor.rhs, !dbg !179

lor.rhs:                                          ; preds = %entry
  %4 = load i32* @flag_pedantic_errors, align 4, !dbg !179, !tbaa !184
  %lnot = icmp eq i32 %4, 0, !dbg !179
  br label %lor.end, !dbg !179

lor.end:                                          ; preds = %entry, %lor.rhs
  %5 = phi i1 [ true, %entry ], [ %lnot, %lor.rhs ]
  %lor.ext = zext i1 %5 to i32, !dbg !179
  call void @set_diagnostic_context(%struct.diagnostic_context* %dc, i8* %msgid, [1 x %struct.__va_list_tag]* %ap, i8* %1, i32 %2, i32 %lor.ext) #4, !dbg !179
  call void @report_diagnostic(%struct.diagnostic_context* %dc) #4, !dbg !185
  call void @llvm.va_end(i8* %arraydecay1), !dbg !186
  call void @llvm.lifetime.end(i64 376, i8* %0) #2, !dbg !187
  ret void, !dbg !187
}
