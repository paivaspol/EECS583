define i8* @cpp_quote_string(i8* %dest, i8* nocapture %src, i32 %len) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{i8* %dest}, i64 0, metadata !137), !dbg !872
  tail call void @llvm.dbg.value(metadata !{i8* %src}, i64 0, metadata !138), !dbg !873
  tail call void @llvm.dbg.value(metadata !{i32 %len}, i64 0, metadata !139), !dbg !874
  %tobool27 = icmp eq i32 %len, 0, !dbg !875
  br i1 %tobool27, label %while.end, label %while.body, !dbg !875

while.body:                                       ; preds = %entry, %while.cond.backedge
  %dec30.in = phi i32 [ %dec30, %while.cond.backedge ], [ %len, %entry ]
  %dest.addr.029 = phi i8* [ %dest.addr.0.be, %while.cond.backedge ], [ %dest, %entry ]
  %src.addr.028 = phi i8* [ %incdec.ptr, %while.cond.backedge ], [ %src, %entry ]
  %dec30 = add i32 %dec30.in, -1, !dbg !875
  %incdec.ptr = getelementptr inbounds i8* %src.addr.028, i64 1, !dbg !876
  tail call void @llvm.dbg.value(metadata !{i8* %incdec.ptr}, i64 0, metadata !138), !dbg !876
  %0 = load i8* %src.addr.028, align 1, !dbg !876, !tbaa !877
  tail call void @llvm.dbg.value(metadata !{i8 %0}, i64 0, metadata !140), !dbg !876
  %conv = zext i8 %0 to i32, !dbg !879
  switch i8 %0, label %if.else [
    i8 92, label %if.then
    i8 34, label %if.then
  ], !dbg !879

if.then:                                          ; preds = %while.body, %while.body
  %incdec.ptr5 = getelementptr inbounds i8* %dest.addr.029, i64 1, !dbg !880
  tail call void @llvm.dbg.value(metadata !{i8* %incdec.ptr5}, i64 0, metadata !137), !dbg !880
  store i8 92, i8* %dest.addr.029, align 1, !dbg !880, !tbaa !877
  %incdec.ptr6 = getelementptr inbounds i8* %dest.addr.029, i64 2, !dbg !882
  tail call void @llvm.dbg.value(metadata !{i8* %incdec.ptr6}, i64 0, metadata !137), !dbg !882
  store i8 %0, i8* %incdec.ptr5, align 1, !dbg !882, !tbaa !877
  br label %while.cond.backedge, !dbg !883

if.else:                                          ; preds = %while.body
  %idxprom = zext i8 %0 to i64, !dbg !884
  %arrayidx = getelementptr inbounds [256 x i16]* @_sch_istable, i64 0, i64 %idxprom, !dbg !884
  %1 = load i16* %arrayidx, align 2, !dbg !884, !tbaa !886
  %and9 = and i16 %1, 16, !dbg !884
  %tobool10 = icmp eq i16 %and9, 0, !dbg !884
  br i1 %tobool10, label %if.else13, label %if.then11, !dbg !884

if.then11:                                        ; preds = %if.else
  %incdec.ptr12 = getelementptr inbounds i8* %dest.addr.029, i64 1, !dbg !887
  tail call void @llvm.dbg.value(metadata !{i8* %incdec.ptr12}, i64 0, metadata !137), !dbg !887
  store i8 %0, i8* %dest.addr.029, align 1, !dbg !887, !tbaa !877
  br label %while.cond.backedge, !dbg !887

while.cond.backedge:                              ; preds = %if.then11, %if.else13, %if.then
  %dest.addr.0.be = phi i8* [ %incdec.ptr6, %if.then ], [ %incdec.ptr12, %if.then11 ], [ %add.ptr, %if.else13 ]
  tail call void @llvm.dbg.value(metadata !{i32 %dec30}, i64 0, metadata !139), !dbg !875
  %tobool = icmp eq i32 %dec30, 0, !dbg !875
  br i1 %tobool, label %while.end, label %while.body, !dbg !875

if.else13:                                        ; preds = %if.else
  %call = tail call i32 (i8*, i8*, ...)* @sprintf(i8* %dest.addr.029, i8* getelementptr inbounds ([6 x i8]* @.str, i64 0, i64 0), i32 %conv) #8, !dbg !888
  %add.ptr = getelementptr inbounds i8* %dest.addr.029, i64 4, !dbg !890
  tail call void @llvm.dbg.value(metadata !{i8* %add.ptr}, i64 0, metadata !137), !dbg !890
  br label %while.cond.backedge

while.end:                                        ; preds = %while.cond.backedge, %entry
  %dest.addr.0.lcssa = phi i8* [ %dest, %entry ], [ %dest.addr.0.be, %while.cond.backedge ]
  ret i8* %dest.addr.0.lcssa, !dbg !891
