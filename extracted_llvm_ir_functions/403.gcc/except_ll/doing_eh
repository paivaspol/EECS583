define i32 @doing_eh(i32 %do_warn) #0 {
entry:
  %0 = load i32* @flag_exceptions, align 4, !tbaa !0
  %tobool = icmp eq i32 %0, 0
  br i1 %tobool, label %if.then, label %return

if.then:                                          ; preds = %entry
  %.b = load i1* @doing_eh.warned, align 1
  %tobool2 = icmp eq i32 %do_warn, 0
  %or.cond = or i1 %.b, %tobool2
  br i1 %or.cond, label %return, label %if.then3

if.then3:                                         ; preds = %if.then
  tail call void (i8*, ...)* @error(i8* getelementptr inbounds ([56 x i8]* @.str, i64 0, i64 0)) #6
  store i1 true, i1* @doing_eh.warned, align 1
  br label %return

return:                                           ; preds = %entry, %if.then, %if.then3
  %retval.0 = phi i32 [ 0, %if.then3 ], [ 0, %if.then ], [ 1, %entry ]
  ret i32 %retval.0
}
