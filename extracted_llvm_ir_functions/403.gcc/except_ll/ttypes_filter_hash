define internal i32 @ttypes_filter_hash(i8* nocapture %pentry) #3 {
entry:
  tail call void @llvm.dbg.value(metadata !{i8* %pentry}, i64 0, metadata !2048), !dbg !3627
  %t = bitcast i8* %pentry to %union.tree_node**, !dbg !3628
  %0 = load %union.tree_node** %t, align 8, !dbg !3628, !tbaa !2232
  %1 = ptrtoint %union.tree_node* %0 to i64, !dbg !3628
  %.tr = trunc i64 %1 to i32, !dbg !3628
  %conv = and i32 %.tr, 262143, !dbg !3628
  ret i32 %conv, !dbg !3628
}
