define void @init_eh() #0 {
entry:
  tail call void @ggc_add_root(i8* bitcast (%struct.htab** @exception_handler_label_map to i8*), i32 1, i32 1, void (i8*)* @mark_ehl_map) #6
  %0 = load i32* @flag_exceptions, align 4, !tbaa !0
  %tobool = icmp eq i32 %0, 0
  br i1 %tobool, label %return, label %if.end

if.end:                                           ; preds = %entry
  %call = tail call %struct.htab* @htab_create(i64 31, i32 (i8*)* @t2r_hash, i32 (i8*, i8*)* @t2r_eq, void (i8*)* null) #6
  store %struct.htab* %call, %struct.htab** @type_to_runtime_map, align 8, !tbaa !3
  tail call void @ggc_add_root(i8* bitcast (%struct.htab** @type_to_runtime_map to i8*), i32 1, i32 8, void (i8*)* @t2r_mark) #6
  br label %return

return:                                           ; preds = %entry, %if.end
  ret void
}
