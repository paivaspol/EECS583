define %struct.rtx_def* @get_exception_pointer(%struct.function* %fun) #0 {
entry:
  %eh = getelementptr inbounds %struct.function* %fun, i64 0, i32 0
  %0 = load %struct.eh_status** %eh, align 8, !tbaa !3
  %exc_ptr1 = getelementptr inbounds %struct.eh_status* %0, i64 0, i32 6
  %1 = load %struct.rtx_def** %exc_ptr1, align 8, !tbaa !3
  %2 = load %struct.function** @cfun, align 8, !tbaa !3
  %cmp = icmp eq %struct.function* %2, %fun
  %tobool = icmp eq %struct.rtx_def* %1, null
  %or.cond = and i1 %cmp, %tobool
  br i1 %or.cond, label %if.then, label %if.end

if.then:                                          ; preds = %entry
  %3 = load i32* @target_flags, align 4, !tbaa !0
  %and = lshr i32 %3, 25
  %4 = and i32 %and, 1
  %5 = or i32 %4, 4
  %call = tail call %struct.rtx_def* @gen_reg_rtx(i32 %5) #6
  %6 = load %struct.eh_status** %eh, align 8, !tbaa !3
  %exc_ptr4 = getelementptr inbounds %struct.eh_status* %6, i64 0, i32 6
  store %struct.rtx_def* %call, %struct.rtx_def** %exc_ptr4, align 8, !tbaa !3
  br label %if.end

if.end:                                           ; preds = %if.then, %entry
  %exc_ptr.0 = phi %struct.rtx_def* [ %call, %if.then ], [ %1, %entry ]
  ret %struct.rtx_def* %exc_ptr.0
}
