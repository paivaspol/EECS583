define void @push_srcloc(i8* %file, i32 %line) #0 {
entry:
  %0 = load %struct.file_stack** @input_file_stack, align 8, !tbaa !4
  %tobool = icmp eq %struct.file_stack* %0, null
  br i1 %tobool, label %if.end, label %if.then

if.then:                                          ; preds = %entry
  %1 = load i8** @input_filename, align 8, !tbaa !4
  %name = getelementptr inbounds %struct.file_stack* %0, i64 0, i32 0
  store i8* %1, i8** %name, align 8, !tbaa !4
  %2 = load i32* @lineno, align 4, !tbaa !0
  %3 = load %struct.file_stack** @input_file_stack, align 8, !tbaa !4
  %line1 = getelementptr inbounds %struct.file_stack* %3, i64 0, i32 2
  store i32 %2, i32* %line1, align 4, !tbaa !0
  br label %if.end

if.end:                                           ; preds = %entry, %if.then
  %call = tail call noalias i8* @xmalloc(i64 24) #11
  %4 = bitcast i8* %call to %struct.file_stack*
  store i8* %file, i8** @input_filename, align 8, !tbaa !4
  %name2 = bitcast i8* %call to i8**
  store i8* %file, i8** %name2, align 8, !tbaa !4
  store i32 %line, i32* @lineno, align 4, !tbaa !0
  %line3 = getelementptr inbounds i8* %call, i64 16
  %5 = bitcast i8* %line3 to i32*
  store i32 %line, i32* %5, align 4, !tbaa !0
  %indent_level = getelementptr inbounds i8* %call, i64 20
  %6 = bitcast i8* %indent_level to i32*
  store i32 0, i32* %6, align 4, !tbaa !0
  %7 = load %struct.file_stack** @input_file_stack, align 8, !tbaa !4
  %next = getelementptr inbounds i8* %call, i64 8
  %8 = bitcast i8* %next to %struct.file_stack**
  store %struct.file_stack* %7, %struct.file_stack** %8, align 8, !tbaa !4
  store %struct.file_stack* %4, %struct.file_stack** @input_file_stack, align 8, !tbaa !4
  %9 = load i32* @input_file_stack_tick, align 4, !tbaa !0
  %inc = add nsw i32 %9, 1
  store i32 %inc, i32* @input_file_stack_tick, align 4, !tbaa !0
  ret void
}
