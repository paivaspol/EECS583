define i32 @do_float_handler(void (i8*)* nocapture %fn, i8* %data) #0 {
entry:
  %buf = alloca [1 x %struct.__jmp_buf_tag], align 16
  call void @llvm.dbg.value(metadata !{void (i8*)* %fn}, i64 0, metadata !741), !dbg !2107
  call void @llvm.dbg.value(metadata !{i8* %data}, i64 0, metadata !742), !dbg !2108
  %0 = bitcast [1 x %struct.__jmp_buf_tag]* %buf to i8*, !dbg !2109
  call void @llvm.lifetime.start(i64 200, i8* %0) #7, !dbg !2109
  call void @llvm.dbg.declare(metadata !{[1 x %struct.__jmp_buf_tag]* %buf}, metadata !743), !dbg !2109
  %arraydecay = getelementptr inbounds [1 x %struct.__jmp_buf_tag]* %buf, i64 0, i64 0, !dbg !2110
  %call = call i32 @_setjmp(%struct.__jmp_buf_tag* %arraydecay) #15, !dbg !2110
  %tobool = icmp eq i32 %call, 0, !dbg !2110
  br i1 %tobool, label %if.end, label %cleanup, !dbg !2110

if.end:                                           ; preds = %entry
  call void @llvm.dbg.value(metadata !{%struct.__jmp_buf_tag* %arraydecay}, i64 0, metadata !2111) #7, !dbg !2113
  store i32 1, i32* @float_handled, align 4, !dbg !2114, !tbaa !2059
  call void @llvm.memcpy.p0i8.p0i8.i64(i8* bitcast ([1 x %struct.__jmp_buf_tag]* @float_handler to i8*), i8* %0, i64 200, i32 16, i1 false) #7, !dbg !2115
  %.b.i = load i1* @float_handler_set, align 1
  br i1 %.b.i, label %set_float_handler.exit, label %if.then3.i, !dbg !2116

if.then3.i:                                       ; preds = %if.end
  %call.i = call void (i32)* (i32, void (i32)*)* @signal(i32 8, void (i32)* @float_signal) #12, !dbg !2117
  store i1 true, i1* @float_handler_set, align 1
  br label %set_float_handler.exit, !dbg !2119

set_float_handler.exit:                           ; preds = %if.end, %if.then3.i
  call void %fn(i8* %data) #12, !dbg !2120
  tail call void @llvm.dbg.value(metadata !2121, i64 0, metadata !2122), !dbg !2124
  br label %cleanup, !dbg !2125

cleanup:                                          ; preds = %entry, %set_float_handler.exit
  %retval.0 = phi i32 [ 1, %set_float_handler.exit ], [ 0, %entry ]
  store i32 0, i32* @float_handled, align 4, !dbg !2126, !tbaa !2059
  call void @llvm.lifetime.end(i64 200, i8* %0) #7, !dbg !2125
  ret i32 %retval.0, !dbg !2127
}
