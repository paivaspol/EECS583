define i8* @fname_as_string(i32 %pretty_p) #0 {
entry:
  %tobool = icmp eq i32 %pretty_p, 0
  %0 = load %union.tree_node** @current_function_decl, align 8, !tbaa !3
  %tobool1 = icmp ne %union.tree_node* %0, null
  br i1 %tobool, label %if.else, label %if.then

if.then:                                          ; preds = %entry
  br i1 %tobool1, label %cond.true, label %if.end9

cond.true:                                        ; preds = %if.then
  %1 = load i8* (%union.tree_node*, i32)** @decl_printable_name, align 8, !tbaa !3
  %call = tail call i8* %1(%union.tree_node* %0, i32 2) #8
  br label %if.end9

if.else:                                          ; preds = %entry
  br i1 %tobool1, label %land.lhs.true, label %if.end9

land.lhs.true:                                    ; preds = %if.else
  %name3 = getelementptr inbounds %union.tree_node* %0, i64 0, i32 0, i32 8
  %2 = load %union.tree_node** %name3, align 8, !tbaa !3
  %tobool4 = icmp eq %union.tree_node* %2, null
  br i1 %tobool4, label %if.end9, label %if.then5

if.then5:                                         ; preds = %land.lhs.true
  %id = getelementptr inbounds %union.tree_node* %2, i64 0, i32 0, i32 1
  %3 = getelementptr inbounds i8** %id, i64 1
  %4 = load i8** %3, align 8, !tbaa !3
  br label %if.end9

if.end9:                                          ; preds = %if.else, %land.lhs.true, %cond.true, %if.then, %if.then5
  %name.0 = phi i8* [ %4, %if.then5 ], [ %call, %cond.true ], [ getelementptr inbounds ([10 x i8]* @.str1, i64 0, i64 0), %if.then ], [ getelementptr inbounds ([1 x i8]* @.str2, i64 0, i64 0), %land.lhs.true ], [ getelementptr inbounds ([1 x i8]* @.str2, i64 0, i64 0), %if.else ]
  ret i8* %name.0
}
