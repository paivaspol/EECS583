define void @c_finish_then() #0 {
entry:
  %0 = load i32* @if_stack_pointer, align 4, !tbaa !0
  %sub = add nsw i32 %0, -1
  %idxprom = sext i32 %sub to i64
  %1 = load %struct.if_elt** @if_stack, align 8, !tbaa !3
  %if_stmt1 = getelementptr inbounds %struct.if_elt* %1, i64 %idxprom, i32 4
  %2 = load %union.tree_node** %if_stmt1, align 8, !tbaa !3
  %chain = getelementptr inbounds %union.tree_node* %2, i64 0, i32 0, i32 0, i32 0
  %3 = load %union.tree_node** %chain, align 8, !tbaa !3
  %operands = getelementptr inbounds %union.tree_node* %2, i64 0, i32 0, i32 2
  %arrayidx2 = getelementptr inbounds i32* %operands, i64 2
  %4 = bitcast i32* %arrayidx2 to %union.tree_node**
  store %union.tree_node* %3, %union.tree_node** %4, align 8, !tbaa !3
  store %union.tree_node* null, %union.tree_node** %chain, align 8, !tbaa !3
  %call = tail call %struct.stmt_tree_s* @current_stmt_tree() #8
  %x_last_stmt = getelementptr inbounds %struct.stmt_tree_s* %call, i64 0, i32 0
  store %union.tree_node* %2, %union.tree_node** %x_last_stmt, align 8, !tbaa !3
  ret void
}
