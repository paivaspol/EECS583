define void @begin_stmt_tree(%union.tree_node** nocapture %t) #0 {
entry:
  %0 = load %union.tree_node** getelementptr inbounds ([32 x %union.tree_node*]* @c_global_trees, i64 0, i64 31), align 8, !tbaa !0
