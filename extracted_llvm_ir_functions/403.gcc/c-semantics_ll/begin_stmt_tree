  store %union.tree_node* %call, %union.tree_node** %t, align 8, !tbaa !0
  %call1 = tail call %struct.stmt_tree_s* @current_stmt_tree() #5
  %x_last_stmt = getelementptr inbounds %struct.stmt_tree_s* %call1, i64 0, i32 0
  store %union.tree_node* %call, %union.tree_node** %x_last_stmt, align 8, !tbaa !0
  %call2 = tail call %struct.stmt_tree_s* @current_stmt_tree() #5
  %x_last_expr_type = getelementptr inbounds %struct.stmt_tree_s* %call2, i64 0, i32 1
  store %union.tree_node* null, %union.tree_node** %x_last_expr_type, align 8, !tbaa !0
  %1 = load i8** @input_filename, align 8, !tbaa !0
