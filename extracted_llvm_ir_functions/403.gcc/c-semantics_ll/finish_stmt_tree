define void @finish_stmt_tree(%union.tree_node** nocapture %t) #0 {
entry:
  %0 = load %union.tree_node** %t, align 8, !tbaa !0
  %chain = getelementptr inbounds %union.tree_node* %0, i64 0, i32 0, i32 0, i32 0
  %1 = load %union.tree_node** %chain, align 8, !tbaa !0
  store %union.tree_node* %1, %union.tree_node** %t, align 8, !tbaa !0
  %call = tail call %struct.stmt_tree_s* @current_stmt_tree() #5
  %x_last_stmt = getelementptr inbounds %struct.stmt_tree_s* %call, i64 0, i32 0
  store %union.tree_node* null, %union.tree_node** %x_last_stmt, align 8, !tbaa !0
  %2 = load %struct.function** @cfun, align 8, !tbaa !0
  %tobool = icmp eq %struct.function* %2, null
  %tobool1 = icmp eq %union.tree_node* %1, null
  %or.cond = or i1 %tobool, %tobool1
  br i1 %or.cond, label %if.end, label %if.then

if.then:                                          ; preds = %entry
  %3 = load i32* @lineno, align 4, !tbaa !4
  %complexity = getelementptr inbounds %union.tree_node* %1, i64 0, i32 0, i32 1
  %4 = bitcast i8** %complexity to i32*
  store i32 %3, i32* %4, align 4, !tbaa !4
  %lang_flag_2 = getelementptr inbounds %union.tree_node* %1, i64 0, i32 0, i32 0, i32 2
  %5 = bitcast [4 x i8]* %lang_flag_2 to i32*
  %bf.load = load i32* %5, align 8
  %bf.set = or i32 %bf.load, 67108864
  store i32 %bf.set, i32* %5, align 8
  br label %if.end

if.end:                                           ; preds = %entry, %if.then
  ret void
}
