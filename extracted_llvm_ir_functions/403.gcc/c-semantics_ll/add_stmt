define %union.tree_node* @add_stmt(%union.tree_node* %t) #0 {
entry:
  %0 = load i8** @input_filename, align 8, !tbaa !0
  %call = tail call %struct.stmt_tree_s* @current_stmt_tree() #5
  %x_last_expr_filename = getelementptr inbounds %struct.stmt_tree_s* %call, i64 0, i32 2
  %1 = load i8** %x_last_expr_filename, align 8, !tbaa !0
  %cmp = icmp eq i8* %0, %1
  br i1 %cmp, label %if.end11, label %if.then

if.then:                                          ; preds = %entry
  %2 = load i8** @input_filename, align 8, !tbaa !0
  %call1 = tail call %struct.stmt_tree_s* @current_stmt_tree() #5
  %x_last_expr_filename2 = getelementptr inbounds %struct.stmt_tree_s* %call1, i64 0, i32 2
  %3 = load i8** %x_last_expr_filename2, align 8, !tbaa !0
  %call3 = tail call i32 @strcmp(i8* %2, i8* %3) #6
  %cmp4 = icmp eq i32 %call3, 0
  %4 = load i8** @input_filename, align 8, !tbaa !0
  %call5 = tail call %struct.stmt_tree_s* @current_stmt_tree() #5
  %x_last_expr_filename6 = getelementptr inbounds %struct.stmt_tree_s* %call5, i64 0, i32 2
  store i8* %4, i8** %x_last_expr_filename6, align 8, !tbaa !0
  br i1 %cmp4, label %if.end11, label %if.then7

if.then7:                                         ; preds = %if.then
  %5 = load i8** @input_filename, align 8, !tbaa !0
  %call8 = tail call %union.tree_node* @get_identifier(i8* %5) #5
  %call9 = tail call %union.tree_node* (i32, ...)* @build_nt(i32 167, %union.tree_node* %call8) #5
  %call10 = tail call %union.tree_node* @add_stmt(%union.tree_node* %call9) #7
  br label %if.end11

if.end11:                                         ; preds = %if.then, %entry, %if.then7
  %call12 = tail call %struct.stmt_tree_s* @current_stmt_tree() #5
  %x_last_stmt = getelementptr inbounds %struct.stmt_tree_s* %call12, i64 0, i32 0
  %6 = load %union.tree_node** %x_last_stmt, align 8, !tbaa !0
  %chain = getelementptr inbounds %union.tree_node* %6, i64 0, i32 0, i32 0, i32 0
  store %union.tree_node* %t, %union.tree_node** %chain, align 8, !tbaa !0
  %call13 = tail call %struct.stmt_tree_s* @current_stmt_tree() #5
  %x_last_stmt14 = getelementptr inbounds %struct.stmt_tree_s* %call13, i64 0, i32 0
  store %union.tree_node* %t, %union.tree_node** %x_last_stmt14, align 8, !tbaa !0
  %call15 = tail call i32 @stmts_are_full_exprs_p() #5
  %call16 = tail call %struct.stmt_tree_s* @current_stmt_tree() #5
  %x_last_stmt17 = getelementptr inbounds %struct.stmt_tree_s* %call16, i64 0, i32 0
  %7 = load %union.tree_node** %x_last_stmt17, align 8, !tbaa !0
  %lang_flag_1 = getelementptr inbounds %union.tree_node* %7, i64 0, i32 0, i32 0, i32 2
  %8 = bitcast [4 x i8]* %lang_flag_1 to i32*
  %bf.load = load i32* %8, align 8
  %bf.value = shl i32 %call15, 25
  %bf.shl = and i32 %bf.value, 33554432
  %bf.clear = and i32 %bf.load, -33554433
  %bf.set = or i32 %bf.clear, %bf.shl
  store i32 %bf.set, i32* %8, align 8
  %9 = load %union.tree_node** @current_function_decl, align 8, !tbaa !0
  %tobool19 = icmp eq %union.tree_node* %9, null
  br i1 %tobool19, label %if.end21, label %if.then20

if.then20:                                        ; preds = %if.end11
  %i = getelementptr inbounds %union.tree_node* %9, i64 0, i32 0, i32 6, i32 0
  %10 = load i64* %i, align 8, !tbaa !3
  %inc = add nsw i64 %10, 1
  store i64 %inc, i64* %i, align 8, !tbaa !3
  br label %if.end21

if.end21:                                         ; preds = %if.end11, %if.then20
  ret %union.tree_node* %t
}
