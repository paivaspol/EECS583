define void @prep_stmt(%union.tree_node* nocapture %t) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %t}, i64 0, metadata !1232), !dbg !1613
  %lang_flag_2 = getelementptr inbounds %union.tree_node* %t, i64 0, i32 0, i32 0, i32 2, !dbg !1614
  %0 = bitcast [4 x i8]* %lang_flag_2 to i32*, !dbg !1614
  %bf.load = load i32* %0, align 8, !dbg !1614
  %bf.clear = and i32 %bf.load, 67108864, !dbg !1614
  %tobool = icmp eq i32 %bf.clear, 0, !dbg !1614
  br i1 %tobool, label %if.then, label %if.end, !dbg !1614

if.then:                                          ; preds = %entry
  %complexity = getelementptr inbounds %union.tree_node* %t, i64 0, i32 0, i32 1, !dbg !1615
  %1 = bitcast i8** %complexity to i32*, !dbg !1615
  %2 = load i32* %1, align 4, !dbg !1615, !tbaa !1276
  store i32 %2, i32* @lineno, align 4, !dbg !1615, !tbaa !1276
  %bf.load2.pre = load i32* %0, align 8, !dbg !1616
  br label %if.end, !dbg !1615

if.end:                                           ; preds = %entry, %if.then
  %bf.load2 = phi i32 [ %bf.load, %entry ], [ %bf.load2.pre, %if.then ]
  %bf.lshr3 = lshr i32 %bf.load2, 25, !dbg !1616
  %bf.clear4 = and i32 %bf.lshr3, 1, !dbg !1616
  %call = tail call %struct.stmt_tree_s* @current_stmt_tree() #6, !dbg !1616
  %stmts_are_full_exprs_p = getelementptr inbounds %struct.stmt_tree_s* %call, i64 0, i32 3, !dbg !1616
  store i32 %bf.clear4, i32* %stmts_are_full_exprs_p, align 4, !dbg !1616, !tbaa !1276
  ret void, !dbg !1617
}
