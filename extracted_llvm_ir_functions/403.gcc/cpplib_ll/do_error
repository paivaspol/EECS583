define internal void @do_error(%struct.cpp_reader* %pfile) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{%struct.cpp_reader* %pfile}, i64 0, metadata !964), !dbg !1807
  tail call fastcc void @do_diagnostic(%struct.cpp_reader* %pfile, i32 3, i32 1) #10, !dbg !1808
  ret void, !dbg !1809
}
