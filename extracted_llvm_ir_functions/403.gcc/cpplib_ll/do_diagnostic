define internal fastcc void @do_diagnostic(%struct.cpp_reader* %pfile, i32 %code, i32 %print_dir) #0 {
entry:
  %call = tail call i32 @_cpp_begin_message(%struct.cpp_reader* %pfile, i32 %code, i32 0, i32 0) #8
  %tobool = icmp eq i32 %call, 0
  br i1 %tobool, label %if.end6, label %if.then

if.then:                                          ; preds = %entry
  %tobool1 = icmp eq i32 %print_dir, 0
  br i1 %tobool1, label %if.end, label %if.then2

if.then2:                                         ; preds = %if.then
  %0 = load %struct._IO_FILE** @stderr, align 8, !tbaa !3
  %directive = getelementptr inbounds %struct.cpp_reader* %pfile, i64 0, i32 11
  %1 = load %struct.directive** %directive, align 8, !tbaa !3
  %name = getelementptr inbounds %struct.directive* %1, i64 0, i32 1
  %2 = load i8** %name, align 8, !tbaa !3
  %call3 = tail call i32 (%struct._IO_FILE*, i8*, ...)* @fprintf(%struct._IO_FILE* %0, i8* getelementptr inbounds ([5 x i8]* @.str19, i64 0, i64 0), i8* %2) #8
  br label %if.end

if.end:                                           ; preds = %if.then, %if.then2
  %prevent_expansion = getelementptr inbounds %struct.cpp_reader* %pfile, i64 0, i32 1, i32 7
  %3 = load i8* %prevent_expansion, align 1, !tbaa !0
  %inc = add i8 %3, 1
  store i8 %inc, i8* %prevent_expansion, align 1, !tbaa !0
  %4 = load %struct._IO_FILE** @stderr, align 8, !tbaa !3
  tail call void @cpp_output_line(%struct.cpp_reader* %pfile, %struct._IO_FILE* %4) #8
  %5 = load i8* %prevent_expansion, align 1, !tbaa !0
  %dec = add i8 %5, -1
  store i8 %dec, i8* %prevent_expansion, align 1, !tbaa !0
  br label %if.end6

if.end6:                                          ; preds = %entry, %if.end
  ret void
}
