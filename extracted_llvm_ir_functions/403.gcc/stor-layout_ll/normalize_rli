define void @normalize_rli(%struct.record_layout_info_s* nocapture %rli) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{%struct.record_layout_info_s* %rli}, i64 0, metadata !1033), !dbg !1392
  %offset = getelementptr inbounds %struct.record_layout_info_s* %rli, i64 0, i32 1, !dbg !1393
  %bitpos = getelementptr inbounds %struct.record_layout_info_s* %rli, i64 0, i32 3, !dbg !1393
  %offset_align = getelementptr inbounds %struct.record_layout_info_s* %rli, i64 0, i32 2, !dbg !1393
  %0 = load i32* %offset_align, align 4, !dbg !1393, !tbaa !1220
  tail call void @normalize_offset(%union.tree_node** %offset, %union.tree_node** %bitpos, i32 %0) #10, !dbg !1393
  ret void, !dbg !1394
}
