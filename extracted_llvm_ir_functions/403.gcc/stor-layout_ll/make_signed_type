define %union.tree_node* @make_signed_type(i32 %precision) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{i32 %precision}, i64 0, metadata !1105), !dbg !1855
  %call = tail call %union.tree_node* @make_node(i32 6) #8, !dbg !1856
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %call}, i64 0, metadata !1106), !dbg !1856
  %type1 = bitcast %union.tree_node* %call to %struct.tree_type*, !dbg !1857
  %precision2 = getelementptr inbounds %struct.tree_type* %type1, i64 0, i32 6, !dbg !1857
  %0 = bitcast [4 x i8]* %precision2 to i32*, !dbg !1857
  %bf.load = load i32* %0, align 4, !dbg !1857
  %bf.value = and i32 %precision, 511, !dbg !1857
  %bf.clear = and i32 %bf.load, -512, !dbg !1857
  %bf.set = or i32 %bf.clear, %bf.value, !dbg !1857
  store i32 %bf.set, i32* %0, align 4, !dbg !1857
  tail call void @fixup_signed_type(%union.tree_node* %call) #10, !dbg !1858
  ret %union.tree_node* %call, !dbg !1859
}
