define i32 @dump_flag(%struct.dump_info* nocapture %di, i32 %flag, %union.tree_node* %node) #4 {
entry:
  %flags = getelementptr inbounds %struct.dump_info* %di, i64 0, i32 2
  %0 = load i32* %flags, align 4, !tbaa !4
  %and = and i32 %0, %flag
  %tobool = icmp eq i32 %and, 0
  br i1 %tobool, label %land.end, label %land.rhs

land.rhs:                                         ; preds = %entry
  %node1 = getelementptr inbounds %struct.dump_info* %di, i64 0, i32 1
  %1 = load %union.tree_node** %node1, align 8, !tbaa !0
  %cmp = icmp ne %union.tree_node* %1, %node
  br label %land.end

land.end:                                         ; preds = %entry, %land.rhs
  %2 = phi i1 [ false, %entry ], [ %cmp, %land.rhs ]
  %land.ext = zext i1 %2 to i32
  ret i32 %land.ext
}
