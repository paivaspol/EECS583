define %union.tree_node* @expand_start_stmt_expr(i32 %has_scope) #0 {
entry:
  tail call void @llvm.dbg.value(metadata !{i32 %has_scope}, i64 0, metadata !1499), !dbg !3260
  %call = tail call %union.tree_node* @make_node(i32 120) #8, !dbg !3261
  tail call void @llvm.dbg.value(metadata !{%union.tree_node* %call}, i64 0, metadata !1500), !dbg !3261
  tail call void @do_pending_stack_adjust() #8, !dbg !3262
  %tobool = icmp eq i32 %has_scope, 0, !dbg !3263
  br i1 %tobool, label %if.else, label %if.then, !dbg !3263

if.then:                                          ; preds = %entry
  tail call void @start_sequence_for_rtl_expr(%union.tree_node* %call) #8, !dbg !3264
  br label %if.end, !dbg !3264

if.else:                                          ; preds = %entry
  tail call void @start_sequence() #8, !dbg !3265
  br label %if.end

if.end:                                           ; preds = %if.else, %if.then
  %0 = load %struct.function** @cfun, align 8, !dbg !3266, !tbaa !2438
  %expr = getelementptr inbounds %struct.function* %0, i64 0, i32 2, !dbg !3266
  %1 = load %struct.expr_status** %expr, align 8, !dbg !3266, !tbaa !2438
  %x_inhibit_defer_pop = getelementptr inbounds %struct.expr_status* %1, i64 0, i32 1, !dbg !3266
  %2 = load i32* %x_inhibit_defer_pop, align 4, !dbg !3266, !tbaa !2561
  %add = add nsw i32 %2, 1, !dbg !3266
  store i32 %add, i32* %x_inhibit_defer_pop, align 4, !dbg !3266, !tbaa !2561
  %stmt = getelementptr inbounds %struct.function* %0, i64 0, i32 1, !dbg !3267
  %3 = load %struct.stmt_status** %stmt, align 8, !dbg !3267, !tbaa !2438
  %x_expr_stmts_for_value = getelementptr inbounds %struct.stmt_status* %3, i64 0, i32 10, !dbg !3267
  %4 = load i32* %x_expr_stmts_for_value, align 4, !dbg !3267, !tbaa !2561
  %inc = add nsw i32 %4, 1, !dbg !3267
  store i32 %inc, i32* %x_expr_stmts_for_value, align 4, !dbg !3267, !tbaa !2561
  %x_last_expr_value = getelementptr inbounds %struct.stmt_status* %3, i64 0, i32 9, !dbg !3268
  store %struct.rtx_def* null, %struct.rtx_def** %x_last_expr_value, align 8, !dbg !3268, !tbaa !2438
  ret %union.tree_node* %call, !dbg !3269
}
