define i32 @in_control_zone_p() #4 {
entry:
  %0 = load %struct.function** @cfun, align 8, !dbg !2568, !tbaa !2438
  %stmt = getelementptr inbounds %struct.function* %0, i64 0, i32 1, !dbg !2568
  %1 = load %struct.stmt_status** %stmt, align 8, !dbg !2568, !tbaa !2438
  %x_cond_stack = getelementptr inbounds %struct.stmt_status* %1, i64 0, i32 2, !dbg !2568
  %2 = load %struct.nesting** %x_cond_stack, align 8, !dbg !2568, !tbaa !2438
  %tobool = icmp eq %struct.nesting* %2, null, !dbg !2568
  br i1 %tobool, label %lor.lhs.false, label %lor.end, !dbg !2568

lor.lhs.false:                                    ; preds = %entry
  %x_loop_stack = getelementptr inbounds %struct.stmt_status* %1, i64 0, i32 3, !dbg !2568
  %3 = load %struct.nesting** %x_loop_stack, align 8, !dbg !2568, !tbaa !2438
  %tobool2 = icmp eq %struct.nesting* %3, null, !dbg !2568
  br i1 %tobool2, label %lor.rhs, label %lor.end, !dbg !2568

lor.rhs:                                          ; preds = %lor.lhs.false
  %x_case_stack = getelementptr inbounds %struct.stmt_status* %1, i64 0, i32 4, !dbg !2568
  %4 = load %struct.nesting** %x_case_stack, align 8, !dbg !2568, !tbaa !2438
  %tobool4 = icmp ne %struct.nesting* %4, null, !dbg !2568
  br label %lor.end, !dbg !2568

lor.end:                                          ; preds = %lor.lhs.false, %entry, %lor.rhs
  %5 = phi i1 [ true, %lor.lhs.false ], [ true, %entry ], [ %tobool4, %lor.rhs ]
  %lor.ext = zext i1 %5 to i32, !dbg !2568
  ret i32 %lor.ext, !dbg !2568
}
