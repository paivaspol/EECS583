define void @save_stack_pointer() #0 {
entry:
  %0 = load %struct.function** @cfun, align 8, !dbg !3838, !tbaa !2438
  %stmt = getelementptr inbounds %struct.function* %0, i64 0, i32 1, !dbg !3838
  %1 = load %struct.stmt_status** %stmt, align 8, !dbg !3838, !tbaa !2438
  %x_block_stack = getelementptr inbounds %struct.stmt_status* %1, i64 0, i32 0, !dbg !3838
  %2 = load %struct.nesting** %x_block_stack, align 8, !dbg !3838, !tbaa !2438
  tail call void @llvm.dbg.value(metadata !{%struct.nesting* %2}, i64 0, metadata !1755), !dbg !3838
  %stack_level = getelementptr inbounds %struct.nesting* %2, i64 0, i32 4, i32 0, i32 1, !dbg !3839
  %3 = load %struct.rtx_def** %stack_level, align 8, !dbg !3839, !tbaa !2438
  %cmp = icmp eq %struct.rtx_def* %3, null, !dbg !3839
  br i1 %cmp, label %if.then, label %if.end, !dbg !3839

if.then:                                          ; preds = %entry
  %next = getelementptr inbounds %struct.nesting* %2, i64 0, i32 1, !dbg !3840
  %4 = load %struct.nesting** %next, align 8, !dbg !3840, !tbaa !2438
  %not.tobool = icmp eq %struct.nesting* %4, null, !dbg !3840
  %cond = zext i1 %not.tobool to i32, !dbg !3840
  %first_insn = getelementptr inbounds %struct.nesting* %2, i64 0, i32 4, i32 0, i32 2, !dbg !3840
  %5 = load %struct.rtx_def** %first_insn, align 8, !dbg !3840, !tbaa !2438
  tail call void @emit_stack_save(i32 %cond, %struct.rtx_def** %stack_level, %struct.rtx_def* %5) #8, !dbg !3840
  %6 = load %struct.function** @cfun, align 8, !dbg !3842, !tbaa !2438
  %stmt6 = getelementptr inbounds %struct.function* %6, i64 0, i32 1, !dbg !3842
  %7 = load %struct.stmt_status** %stmt6, align 8, !dbg !3842, !tbaa !2438
  %x_stack_block_stack = getelementptr inbounds %struct.stmt_status* %7, i64 0, i32 1, !dbg !3842
  store %struct.nesting* %2, %struct.nesting** %x_stack_block_stack, align 8, !dbg !3842, !tbaa !2438
  br label %if.end, !dbg !3843

if.end:                                           ; preds = %if.then, %entry
  ret void, !dbg !3844
}
