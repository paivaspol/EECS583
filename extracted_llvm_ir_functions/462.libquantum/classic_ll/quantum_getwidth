define i32 @quantum_getwidth(i32 %n) #0 {
entry:
  br label %for.cond

for.cond:                                         ; preds = %for.cond, %entry
  %i.0 = phi i32 [ 1, %entry ], [ %inc, %for.cond ]
  %shl = shl i32 1, %i.0
  %cmp = icmp slt i32 %shl, %n
  %inc = add nsw i32 %i.0, 1
  br i1 %cmp, label %for.cond, label %for.end

for.end:                                          ; preds = %for.cond
  ret i32 %i.0
}
