define void @Perl_pmflag(i32* nocapture %pmfl, i32 %ch) #0 {
entry:
  switch i32 %ch, label %if.end29 [
    i32 105, label %if.then
    i32 103, label %if.then2
    i32 99, label %if.then6
    i32 111, label %if.then10
    i32 109, label %if.then14
    i32 115, label %if.then18
    i32 120, label %if.then22
  ]

if.then:                                          ; preds = %entry
  %0 = load i32* %pmfl, align 4, !tbaa !3
  %or = or i32 %0, 16384
  store i32 %or, i32* %pmfl, align 4, !tbaa !3
  br label %if.end29

if.then2:                                         ; preds = %entry
  %1 = load i32* %pmfl, align 4, !tbaa !3
  %or3 = or i32 %1, 256
  store i32 %or3, i32* %pmfl, align 4, !tbaa !3
  br label %if.end29

if.then6:                                         ; preds = %entry
  %2 = load i32* %pmfl, align 4, !tbaa !3
  %or7 = or i32 %2, 512
  store i32 %or7, i32* %pmfl, align 4, !tbaa !3
  br label %if.end29

if.then10:                                        ; preds = %entry
  %3 = load i32* %pmfl, align 4, !tbaa !3
  %or11 = or i32 %3, 128
  store i32 %or11, i32* %pmfl, align 4, !tbaa !3
  br label %if.end29

if.then14:                                        ; preds = %entry
  %4 = load i32* %pmfl, align 4, !tbaa !3
  %or15 = or i32 %4, 4096
  store i32 %or15, i32* %pmfl, align 4, !tbaa !3
  br label %if.end29

if.then18:                                        ; preds = %entry
  %5 = load i32* %pmfl, align 4, !tbaa !3
  %or19 = or i32 %5, 8192
  store i32 %or19, i32* %pmfl, align 4, !tbaa !3
  br label %if.end29

if.then22:                                        ; preds = %entry
  %6 = load i32* %pmfl, align 4, !tbaa !3
  %or23 = or i32 %6, 32768
  store i32 %or23, i32* %pmfl, align 4, !tbaa !3
  br label %if.end29

if.end29:                                         ; preds = %entry, %if.then2, %if.then10, %if.then18, %if.then22, %if.then14, %if.then6, %if.then
  ret void
}
