define i32 @Perl_yywarn(i8* %s) #0 {
entry:
  %0 = load volatile i32* @PL_in_eval, align 4, !tbaa !3
  %or = or i32 %0, 2
  store volatile i32 %or, i32* @PL_in_eval, align 4, !tbaa !3
  %call = tail call i32 @Perl_yyerror(i8* %s) #7
  %1 = load volatile i32* @PL_in_eval, align 4, !tbaa !3
  %and = and i32 %1, -3
  store volatile i32 %and, i32* @PL_in_eval, align 4, !tbaa !3
  ret i32 0
}
