define internal fastcc i8* @S_filter_gets(%struct.sv* %sv, %struct._PerlIO** %fp, i64 %append) #0 {
entry:
  %0 = load %struct.av** @PL_rsfp_filters, align 8, !tbaa !0
  %tobool = icmp eq %struct.av* %0, null
  br i1 %tobool, label %if.else5, label %if.then

if.then:                                          ; preds = %entry
  %tobool1 = icmp eq i64 %append, 0
  br i1 %tobool1, label %if.then2, label %if.end

if.then2:                                         ; preds = %if.then
  %sv_any = getelementptr inbounds %struct.sv* %sv, i64 0, i32 0
  %1 = load i8** %sv_any, align 8, !tbaa !0
  %xpv_cur = getelementptr inbounds i8* %1, i64 8
  %2 = bitcast i8* %xpv_cur to i64*
  store i64 0, i64* %2, align 8, !tbaa !4
  br label %if.end

if.end:                                           ; preds = %if.then, %if.then2
  %call = tail call i32 @Perl_filter_read(i32 0, %struct.sv* %sv, i32 0) #7
  %cmp = icmp sgt i32 %call, 0
  br i1 %cmp, label %if.then3, label %return

if.then3:                                         ; preds = %if.end
  %sv_any4 = getelementptr inbounds %struct.sv* %sv, i64 0, i32 0
  %3 = load i8** %sv_any4, align 8, !tbaa !0
  %xpv_pv = bitcast i8* %3 to i8**
  %4 = load i8** %xpv_pv, align 8, !tbaa !0
  br label %return

if.else5:                                         ; preds = %entry
  %conv = trunc i64 %append to i32
  %call6 = tail call i8* @Perl_sv_gets(%struct.sv* %sv, %struct._PerlIO** %fp, i32 %conv) #6
  br label %return

return:                                           ; preds = %if.end, %if.else5, %if.then3
  %retval.0 = phi i8* [ %4, %if.then3 ], [ %call6, %if.else5 ], [ null, %if.end ]
  ret i8* %retval.0
}
