  br i1 %cmp27, label %return, label %if.then29

if.then29:                                        ; preds = %while.end
  %tobool30 = icmp eq i8** %p, null
  br i1 %tobool30, label %return, label %if.then31

if.then31:                                        ; preds = %if.then29
