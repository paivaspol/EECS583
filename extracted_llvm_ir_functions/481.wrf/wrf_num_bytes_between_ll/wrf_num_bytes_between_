define i32 @wrf_num_bytes_between_(i8* %a, i8* %b, i32* nocapture %n) #0 {
  %1 = ptrtoint i8* %a to i64
  %2 = ptrtoint i8* %b to i64
  %3 = sub i64 %1, %2
  %4 = trunc i64 %3 to i32
  %5 = icmp slt i32 %4, 0
  %6 = sub nsw i32 0, %4
  %. = select i1 %5, i32 %6, i32 %4
  store i32 %., i32* %n, align 4, !tbaa !2
  ret i32 undef
}
