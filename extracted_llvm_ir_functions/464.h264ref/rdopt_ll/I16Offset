define i32 @I16Offset(i32 %cbp, i32 %i16mode) #6 {
entry:
  tail call void @llvm.dbg.value(metadata !{i32 %cbp}, i64 0, metadata !529), !dbg !2174
  tail call void @llvm.dbg.value(metadata !{i32 %i16mode}, i64 0, metadata !530), !dbg !2174
  %and = and i32 %cbp, 15, !dbg !2175
  %tobool = icmp ne i32 %and, 0, !dbg !2175
  %cond = select i1 %tobool, i32 13, i32 1, !dbg !2175
  %and1 = lshr i32 %cbp, 2, !dbg !2175
  %shr4 = and i32 %and1, 12, !dbg !2175
  %add = add i32 %shr4, %i16mode, !dbg !2175
  %add2 = add i32 %add, %cond, !dbg !2175
  ret i32 %add2, !dbg !2175
}
