  %size_y = getelementptr inbounds %struct.storable_picture* %p, i64 0, i32 19, !dbg !800
  %0 = load i32* %size_y, align 4, !dbg !800, !tbaa !752
  %cmp50 = icmp sgt i32 %0, 0, !dbg !800
