  %conv25 = sext i32 %28 to i64, !dbg !808
  %mul26 = shl nsw i64 %conv25, 1, !dbg !808
  tail call void @llvm.memset.p0i8.i64(i8* %24, i8 %27, i64 %mul26, i32 2, i1 false), !dbg !808
