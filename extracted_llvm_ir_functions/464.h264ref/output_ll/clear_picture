  %21 = load i16**** %imgUV20, align 8, !dbg !808, !tbaa !739
  %arrayidx21 = getelementptr inbounds i16*** %21, i64 1, !dbg !808
  %22 = load i16*** %arrayidx21, align 8, !dbg !808, !tbaa !739
