  %indvars.iv.next53 = add i64 %indvars.iv52, 1, !dbg !803
  %19 = load i32* %size_y_cr, align 4, !dbg !803, !tbaa !752
  %20 = trunc i64 %indvars.iv.next53 to i32, !dbg !803
