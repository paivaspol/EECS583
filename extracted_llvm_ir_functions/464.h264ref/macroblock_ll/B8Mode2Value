define i32 @B8Mode2Value(i32 %b8mode, i32 %b8pdir) #3 {
entry:
  tail call void @llvm.dbg.value(metadata !{i32 %b8mode}, i64 0, metadata !827), !dbg !3022
  tail call void @llvm.dbg.value(metadata !{i32 %b8pdir}, i64 0, metadata !828), !dbg !3022
  %0 = load %struct.ImageParameters** @img, align 8, !dbg !3023, !tbaa !1576
  %type = getelementptr inbounds %struct.ImageParameters* %0, i64 0, i32 6, !dbg !3023
  %1 = load i32* %type, align 4, !dbg !3023, !tbaa !1579
  %cmp = icmp eq i32 %1, 1, !dbg !3023
  br i1 %cmp, label %if.else, label %if.then, !dbg !3023

if.then:                                          ; preds = %entry
  %sub = add nsw i32 %b8mode, -4, !dbg !3024
  br label %return, !dbg !3024

if.else:                                          ; preds = %entry
  %idxprom = sext i32 %b8mode to i64, !dbg !3026
  %arrayidx = getelementptr inbounds [8 x i32]* @B8Mode2Value.b8start, i64 0, i64 %idxprom, !dbg !3026
  %2 = load i32* %arrayidx, align 4, !dbg !3026, !tbaa !1579
  %arrayidx2 = getelementptr inbounds [8 x i32]* @B8Mode2Value.b8inc, i64 0, i64 %idxprom, !dbg !3026
  %3 = load i32* %arrayidx2, align 4, !dbg !3026, !tbaa !1579
  %mul = mul nsw i32 %3, %b8pdir, !dbg !3026
  %add = add nsw i32 %mul, %2, !dbg !3026
  br label %return, !dbg !3026

return:                                           ; preds = %if.else, %if.then
  %retval.0 = phi i32 [ %sub, %if.then ], [ %add, %if.else ]
  ret i32 %retval.0, !dbg !3028
}
