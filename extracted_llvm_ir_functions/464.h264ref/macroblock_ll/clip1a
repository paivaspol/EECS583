define i32 @clip1a(i32 %a) #3 {
entry:
  tail call void @llvm.dbg.value(metadata !{i32 %a}, i64 0, metadata !85), !dbg !1608
  %0 = load %struct.ImageParameters** @img, align 8, !dbg !1609, !tbaa !1576
  %max_imgpel_value = getelementptr inbounds %struct.ImageParameters* %0, i64 0, i32 155, !dbg !1609
  %1 = load i32* %max_imgpel_value, align 4, !dbg !1609, !tbaa !1579
  %cmp = icmp slt i32 %1, %a, !dbg !1609
  br i1 %cmp, label %cond.end5, label %cond.false, !dbg !1609

cond.false:                                       ; preds = %entry
  %cmp2 = icmp slt i32 %a, 0, !dbg !1609
  %cond = select i1 %cmp2, i32 0, i32 %a, !dbg !1609
  br label %cond.end5, !dbg !1609

cond.end5:                                        ; preds = %entry, %cond.false
  %cond6 = phi i32 [ %cond, %cond.false ], [ %1, %entry ], !dbg !1609
  ret i32 %cond6, !dbg !1609
}
