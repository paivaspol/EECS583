define i32 @sign(i32 %a, i32 %b) #7 {
entry:
  tail call void @llvm.dbg.value(metadata !{i32 %a}, i64 0, metadata !876), !dbg !3569
  tail call void @llvm.dbg.value(metadata !{i32 %b}, i64 0, metadata !877), !dbg !3569
  %cmp = icmp slt i32 %a, 0, !dbg !3570
  %sub = sub nsw i32 0, %a, !dbg !3570
  %cond = select i1 %cmp, i32 %sub, i32 %a, !dbg !3570
  tail call void @llvm.dbg.value(metadata !{i32 %cond}, i64 0, metadata !878), !dbg !3570
  %cmp1 = icmp sgt i32 %b, -1, !dbg !3571
  %sub2 = sub nsw i32 0, %cond, !dbg !3572
  %retval.0 = select i1 %cmp1, i32 %cond, i32 %sub2, !dbg !3571
  ret i32 %retval.0, !dbg !3573
}
