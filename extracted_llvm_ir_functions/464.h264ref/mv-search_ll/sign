define i32 @sign(i32 %a, i32 %b) #7 {
  tail call void @llvm.dbg.value(metadata i32 %a, i64 0, metadata !894, metadata !1466), !dbg !7453
  tail call void @llvm.dbg.value(metadata i32 %b, i64 0, metadata !895, metadata !1466), !dbg !7454
  %1 = icmp slt i32 %a, 0, !dbg !7455
  %2 = sub nsw i32 0, %a, !dbg !7455
  %3 = select i1 %1, i32 %2, i32 %a, !dbg !7455
  tail call void @llvm.dbg.value(metadata i32 %3, i64 0, metadata !896, metadata !1466), !dbg !7456
  %4 = icmp sgt i32 %b, -1, !dbg !7457
  %5 = sub nsw i32 0, %3, !dbg !7459
  %.0 = select i1 %4, i32 %3, i32 %5, !dbg !7460
  ret i32 %.0, !dbg !7461
}
