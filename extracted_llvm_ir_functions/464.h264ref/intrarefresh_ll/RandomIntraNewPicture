; <label>:7                                       ; preds = %.lr.ph, %7
  %indvars.iv = phi i64 [ 0, %.lr.ph ], [ %indvars.iv.next, %7 ]
  %i.01 = phi i32 [ %3, %.lr.ph ], [ %14, %7 ]
  %8 = load i32* @NumberOfMBs, align 4, !dbg !663, !tbaa !593
  %9 = srem i32 %i.01, %8, !dbg !664
