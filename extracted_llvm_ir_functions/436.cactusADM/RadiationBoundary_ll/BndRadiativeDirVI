  %call7 = tail call fastcc i32 @ApplyBndRadiative(%struct.cGH* %GH, i32 %stencil_size, i32* null, i32 %dir, double %var0, double %speed, i32 %vi_to, i32 %vi_from, i32 1) #7, !dbg !2131
