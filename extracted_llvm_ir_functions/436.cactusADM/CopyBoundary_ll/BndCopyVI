  tail call void @llvm.dbg.value(metadata !{%struct.cGH* %GH}, i64 0, metadata !77), !dbg !445
  tail call void @llvm.dbg.value(metadata !{i32* %stencil}, i64 0, metadata !78), !dbg !446
