  %11 = tail call i32 @PUGH_SetupPGExtrasStaggering(i32 %dim, i32* undef, i32 undef, i32* undef, i32* undef, i32 undef, i32* undef, i32 %this_proc, %struct.PGExtras* %2) #8
  br label %12

; <label>:12                                      ; preds = %4, %0, %7
  ret %struct.PGExtras* %2
}
