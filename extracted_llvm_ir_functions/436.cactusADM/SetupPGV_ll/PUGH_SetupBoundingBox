if.end40:                                         ; preds = %if.else, %if.then34, %if.then14
  %arrayidx44 = getelementptr inbounds i32* %nprocs, i64 %indvars.iv144, !dbg !980
  %12 = load i32* %arrayidx44, align 4, !dbg !980, !tbaa !381
  %sub45 = add nsw i32 %12, -1, !dbg !980
