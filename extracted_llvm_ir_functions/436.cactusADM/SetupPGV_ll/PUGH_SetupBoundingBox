  tail call void @llvm.dbg.value(metadata !{i32* %nprocs}, i64 0, metadata !208), !dbg !941
  tail call void @llvm.dbg.value(metadata !{%struct.PGExtras* %this}, i64 0, metadata !209), !dbg !942
