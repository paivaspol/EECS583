define i32 @PUGH_SetupBoundingBox(i32 %dim, i32* nocapture %perme, i32 %stagger, i32* nocapture %sh, i32* nocapture %nghosts, i32 %total_procs, i32* %nprocs, %struct.PGExtras* nocapture %this) #1 {
