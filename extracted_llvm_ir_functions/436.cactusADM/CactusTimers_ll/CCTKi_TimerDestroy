define internal fastcc void @CCTKi_TimerDestroy(i32 %this_timer, %struct.t_Timer* %timer) #1 {
  tail call void @llvm.dbg.value(metadata i32 %this_timer, i64 0, metadata !295, metadata !354), !dbg !560
