  %inc = add nsw i32 %i.010, 1, !dbg !263
  %tobool324 = icmp eq i8 %6, 0, !dbg !268
  br i1 %tobool324, label %while.end43, label %land.rhs, !dbg !268

while.cond30:                                     ; preds = %land.rhs
  %8 = load i8* %incdec.ptr41, align 1, !dbg !268, !tbaa !152
