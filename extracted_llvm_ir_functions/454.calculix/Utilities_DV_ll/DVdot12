  %cmp1 = icmp eq double* %row0, null
  %or.cond = or i1 %cmp, %cmp1
  %cmp3 = icmp eq double* %col0, null
  %or.cond32 = or i1 %or.cond, %cmp3
  %cmp5 = icmp eq double* %col1, null
