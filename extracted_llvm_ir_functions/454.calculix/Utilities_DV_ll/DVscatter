  %arrayidx = getelementptr inbounds double* %x, i64 %indvars.iv, !dbg !1061
  %1 = load double* %arrayidx, align 8, !dbg !1061, !tbaa !635
  %arrayidx8 = getelementptr inbounds i32* %index, i64 %indvars.iv, !dbg !1061
