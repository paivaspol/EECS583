define i32 @IVL_sizeOf(%struct._IVL* %ivl) #0 {
entry:
  %cmp = icmp eq %struct._IVL* %ivl, null
  br i1 %cmp, label %if.then, label %if.end

if.then:                                          ; preds = %entry
