if.then2:                                         ; preds = %if.end
  %mul = mul i32 %1, 12
  %add = add i32 %mul, 48
  %type = getelementptr inbounds %struct._IVL* %ivl, i64 0, i32 0
  %2 = load i32* %type, align 4, !tbaa !3
  %cmp6 = icmp eq i32 %2, 2
