define void @strip_comment(i8* %line) #3 {
  %1 = icmp eq i8* %line, null
  br i1 %1, label %6, label %2

; <label>:2                                       ; preds = %0
  %3 = tail call i8* @strchr(i8* %line, i32 59) #10
  %4 = icmp eq i8* %3, null
  br i1 %4, label %6, label %5

; <label>:5                                       ; preds = %2
  store i8 0, i8* %3, align 1, !tbaa !13
  br label %6

; <label>:6                                       ; preds = %2, %0, %5
  ret void
}
