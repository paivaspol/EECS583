define void @_where(i8* nocapture readnone %file, i32 %line) #4 {
  %.b = load i1* @_where.bFirst, align 1
  br i1 %.b, label %2, label %1

; <label>:1                                       ; preds = %0
  store i1 true, i1* @_where.bFirst, align 1
  br label %2

; <label>:2                                       ; preds = %1, %0
  ret void
}
