    i32 0, label %sw.bb29
    i32 2, label %sw.bb31
  ], !dbg !466

sw.bb13:                                          ; preds = %if.end11
  %cmp14 = icmp eq i32 %add, 0, !dbg !467
  br i1 %cmp14, label %return, label %if.end17, !dbg !467

if.end17:                                         ; preds = %sw.bb13
  %cmp18 = icmp eq i8* %0, null, !dbg !469
