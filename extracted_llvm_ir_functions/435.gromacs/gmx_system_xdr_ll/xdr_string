sw.epilog:                                        ; preds = %entry, %if.end4
  %call5 = call i32 @xdr_u_int(%struct.XDR* %xdrs, i32* %size) #7, !dbg !463
  %tobool = icmp eq i32 %call5, 0, !dbg !463
