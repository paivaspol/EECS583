  store volatile i8 1, i8* inttoptr (i64 2952794312 to i8*), align 8, !tbaa !2
  br label %1

; <label>:1                                       ; preds = %1, %0
  %2 = load volatile i8* inttoptr (i64 2952794312 to i8*), align 8, !tbaa !2
  %3 = and i8 %2, 1
  %4 = icmp eq i8 %3, 0
  br i1 %4, label %5, label %1

