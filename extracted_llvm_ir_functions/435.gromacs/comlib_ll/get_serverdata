; <label>:2                                       ; preds = %.preheader, %2
  %3 = load volatile i8* inttoptr (i64 2952794304 to i8*), align 64, !dbg !63, !tbaa !42
  %4 = and i8 %3, 1, !dbg !65
  %5 = icmp eq i8 %4, 0, !dbg !65
  br i1 %5, label %2, label %get_serverbyte.exit, !dbg !66

