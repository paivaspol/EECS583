  %1 = icmp sgt i32 %size, 0, !dbg !59
  br i1 %1, label %.preheader, label %._crit_edge, !dbg !62

.preheader:                                       ; preds = %0, %get_serverbyte.exit
  %p.02 = phi i8* [ %7, %get_serverbyte.exit ], [ %data, %0 ]
  %.01 = phi i32 [ %8, %get_serverbyte.exit ], [ %size, %0 ]
