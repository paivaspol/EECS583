define float @rando(i32* nocapture %ig) #3 {
  %1 = load i32* %ig, align 4, !tbaa !14
  %ispos = icmp sgt i32 %1, -1
  %neg = sub i32 0, %1
  %2 = select i1 %ispos, i32 %1, i32 %neg
  %3 = srem i32 %2, 100000000
  %4 = sdiv i32 %3, 10000
  %5 = srem i32 %3, 10000
  %6 = mul nsw i32 %4, 5821
  %7 = mul nsw i32 %5, 3141
  %8 = add nsw i32 %7, %6
  %9 = srem i32 %8, 10000
  %10 = mul nsw i32 %9, 10000
  %11 = mul nsw i32 %5, 5821
  %12 = add i32 %11, 1
  %13 = add i32 %12, %10
  %14 = srem i32 %13, 100000000
  %15 = sdiv i32 %14, 10
  %16 = sitofp i32 %15 to float
  %17 = fmul float %16, 1.000000e+01
  %18 = fdiv float %17, 1.000000e+08
  %19 = fcmp ole float %18, 0.000000e+00
  %20 = fcmp ogt float %18, 1.000000e+00
  %or.cond = or i1 %19, %20
  %. = select i1 %or.cond, float 0.000000e+00, float %18
  store i32 %14, i32* %ig, align 4, !tbaa !14
  ret float %.
}
