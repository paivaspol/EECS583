; <label>:5                                       ; preds = %0
  %6 = getelementptr inbounds %struct.__sFILE* %_p, i64 0, i32 6, !dbg !320
  %7 = load i32* %6, align 4, !dbg !320, !tbaa !321
  %8 = icmp sle i32 %2, %7, !dbg !322
  %sext.mask = and i32 %_c, 255
