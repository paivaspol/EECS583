  %2 = load i32* %1, align 4, !dbg !307, !tbaa !308
  %3 = add nsw i32 %2, -1, !dbg !307
  store i32 %3, i32* %1, align 4, !dbg !307, !tbaa !308
  %4 = icmp sgt i32 %2, 0, !dbg !317
