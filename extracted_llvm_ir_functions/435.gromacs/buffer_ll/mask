define i32 @mask(i32 %i) #2 {
  %1 = and i32 %i, 255
  %2 = shl i32 %i, 8
  %3 = add i32 %2, 256
  %4 = and i32 %3, 65280
  %5 = shl i32 %i, 16
  %6 = add i32 %5, 131072
  %7 = and i32 %6, 16711680
  %8 = shl i32 %i, 24
  %9 = add i32 %8, 50331648
  %10 = or i32 %9, %1
  %11 = or i32 %10, %4
  %12 = or i32 %11, %7
  ret i32 %12
}
