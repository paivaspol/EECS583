define i32 @mask(i32 %i) #0 {
entry:
  %and = and i32 %i, 255
  %add = shl i32 %i, 8
  %and1 = add i32 %add, 256
  %shl = and i32 %and1, 65280
  %add2 = shl i32 %i, 16
  %and3 = add i32 %add2, 131072
  %shl4 = and i32 %and3, 16711680
  %add6 = shl i32 %i, 24
  %shl8 = add i32 %add6, 50331648
  %or = or i32 %shl8, %and
  %or5 = or i32 %or, %shl
  %or9 = or i32 %or5, %shl4
  ret i32 %or9
}
