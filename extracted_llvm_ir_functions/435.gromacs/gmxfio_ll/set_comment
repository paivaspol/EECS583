define void @set_comment(i8* readonly %comment) #1 {
  %1 = icmp eq i8* %comment, null
  br i1 %1, label %4, label %2

; <label>:2                                       ; preds = %0
  %3 = tail call i8* @strdup(i8* %comment) #9
  store i8* %3, i8** @add_comment, align 8, !tbaa !15
  br label %4

; <label>:4                                       ; preds = %0, %2
  ret void
}
