define float @sign(float %x, float %y) #4 {
  %1 = fcmp olt float %y, 0.000000e+00
  %fabsf = tail call float @fabsf(float %x) #5
  br i1 %1, label %2, label %4

; <label>:2                                       ; preds = %0
  %3 = fsub float -0.000000e+00, %fabsf
  br label %4

; <label>:4                                       ; preds = %0, %2
  %.0 = phi float [ %3, %2 ], [ %fabsf, %0 ]
  ret float %.0
}
