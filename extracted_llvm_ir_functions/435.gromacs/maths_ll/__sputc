; <label>:5                                       ; preds = %0
  %6 = getelementptr inbounds %struct.__sFILE* %_p, i64 0, i32 6, !dbg !475
  %7 = load i32* %6, align 4, !dbg !475, !tbaa !476
  %8 = icmp sle i32 %2, %7, !dbg !477
