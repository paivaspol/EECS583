define void @update_grps(i32 %start, i32 %homenr, %struct.t_groups* nocapture %grps, %struct.t_grpopts* nocapture %opts, [3 x float]* nocapture %v, %struct.t_mdatoms* nocapture %md, i32 %bNEMD) #0 {
