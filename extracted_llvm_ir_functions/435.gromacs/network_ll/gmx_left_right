define void @gmx_left_right(i32 %nnodes, i32 %nodeid, i32* nocapture %left, i32* nocapture %right) #3 {
  %1 = add i32 %nnodes, -1
  %2 = add i32 %1, %nodeid
  %3 = srem i32 %2, %nnodes
  store i32 %3, i32* %left, align 4, !tbaa !14
  %4 = add nsw i32 %nodeid, 1
  %5 = srem i32 %4, %nnodes
  store i32 %5, i32* %right, align 4, !tbaa !14
  ret void
}
