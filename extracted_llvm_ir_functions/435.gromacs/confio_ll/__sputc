  store i8 %11, i8* %13, align 1, !dbg !719, !tbaa !720
  br label %17, !dbg !721

; <label>:15                                      ; preds = %5
  %16 = tail call i32 @__swbuf(i32 %_c, %struct.__sFILE* %_p) #8, !dbg !722
  br label %17, !dbg !723

; <label>:17                                      ; preds = %15, %10
  %.0 = phi i32 [ %.pre-phi, %10 ], [ %16, %15 ]
  ret i32 %.0, !dbg !724
